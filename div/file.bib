@Article{Ingolfsson2004,
  author       = {Ing{\'{o}}lfsson, {\'{O}}lafur},
  title        = {{Quaternary glacial and climate history of Antarctica}},
  year         = {2004},
  volume       = {2},
  number       = {PART C},
  pages        = {3--43},
  issn         = {1571-0866},
  abstract     = {The Antarctic Ice Sheet, containing 25-30 x 106 km 3 of glacial ice (Drewry et al., 1982; Vaughan 2000), is the largest glacial system on Earth. The western part of the present Antarctic Ice Sheet (West Antarctic Ice Sheet, WAIS), west of the Transantarctic Mountains (Fig. 1), is largely marine-based, currently very dynamic and discharges huge volumes of ice to floating ice shelves through large ice-stream systems. To the east of the Transantarctic Mountains the current East Antarctic Ice Sheet (EAIS) is predominantly terrestrial, relatively stable and has limited discharge into surrounding ice shelves (Fig. 2). The WAIS and EAIS contain about 3.3 x 106 km 3 and 26 x 106 km 3 of ice, respectively (Drewry, 1983; Denton et al., 1991). The Antarctic Ice Sheet, potentially containing 57-66 m of sea level equivalent (Denton et al., 1991; Vaughan 2000), is a critical factor in regulating, modifying and forcing the global climate and oceanographic system. Throughout most of the late Cenozoic the Antarctic Ice Sheet has driven global eustasy and deep-ocean circulation, and acted as a regulator of global climate (Anderson, 1999). However, the influences of Antarctic Ice Sheet fluctuations in the Quaternary history of global climate, after initiation of major glaciations in the Northern Hemisphere, are not yet well understood. More than 98{\%} of the Antarctic continent is today covered by glacier ice, and the potential on land for obtaining high-resolution geological data pertaining to its glacial history is poor. A fundamental question is what caused the glacial fluctuations observed in the records? Antarctic glaciers respond both to global sea level fluctuations, mainly controlled during the Quaternary by developments in the Northern Hemisphere, and to Southern Hemisphere climate changes. A good under- standing of the late Quaternary glacial and climate history of Antarctica will constrain the contribution of Antarctic ice to the global sea-level- and marine oxygen-isotope records, and is important for understanding the relative timing of climate changes between the polar hemispheres (Denton et al., 1989; Clapperton {\&} Sugden, 1990; Colhoun et al., 1992; Moriwaki et aL, 1992; Quilty, 1992; Blunier et al., 1998; Steig et al., 1998).},
  doi          = {10.1016/S1571-0866(04)80109-X},
  isbn         = {1571-0866},
  journaltitle = {Developments in Quaternary Science},
}

@Misc{Ingolfsson1997,
  author    = {Ing{\'{o}}lfsson, {\'{O}}lafur and Bj{\"{o}}rck, Svante and Haflidason, Haflidi and Rundgren, Mats},
  title     = {{Glacial and climatic events in {Iceland} reflecting regional North Atlantic climatic shifts during the pleistocene-holocene transition}},
  year      = {1997},
  abstract  = {This paper presents a summary of the evidence for glacial and climatic changes during the late Pleistocene-early Holocene transition in Iceland. The deglaciation during the Bolling-Allerod event was interrupted by a short-lived Older Dryas glacial advance. A biostratigraphical record from northern Iceland shows significant climate warming in late Allerod, when mean July temperatures were at least as warm as those of today. An abrupt cooling marked the beginning of the Younger Dryas event. It was characterised by a cold and stable polar climate and an extensive glaciation, before the postglacial warming of climate set in. The Icelandic paleoclimatic record is discussed in the light of climatic oscillations recorded from the GRIP ice-core, from the Greenland Inland Ice, and with reference to major shifts in the oceanic front systems, recorded in the Troll 8903 marine sediment core from the North Sea. The Vedde Ash gives a unique opportunity to address the chronological problems and correlate event stratigraphies of the different proxies. It is concluded that the Icelandic record of glacial and climatic changes during the late Pleistocene-early Holocene transition largely reflects the climatic development in the North Atlantic region.},
  booktitle = {Quaternary Science Reviews},
  doi       = {10.1016/S0277-3791(97)00007-3},
  isbn      = {02773791},
  issn      = {0277-3791},
  number    = {10},
  pages     = {1135--1144},
  pmid      = {2086383},
  volume    = {16},
}

@Article{Ingolfsson2003,
  author       = {Ing{\'{o}}lfsson, {\'{O}}lafur and Lokrantz, Hanna},
  title        = {{Massive ground ice body of glacial origin at Yugorski Peninsula, Arctic Russia}},
  year         = {2003},
  volume       = {14},
  number       = {3},
  pages        = {199--215},
  issn         = {1045-6740},
  abstract     = {A massive ground ice body at Cape Shpindler on Yugorski Peninsula, southern Kara Sea coast, Russia, was studied with regard to large-scale internal structures, its stratigraphic context and contacts to surrounding sediments, in order to highlight its origin. The massive ground ice contains deformation structures and deformed sediment rafts that show a consistent direction of deforming force. It is bounded upwards with a sharp and unconformable thaw contact to overlying till. The stratigraphical and structural evidence suggests that the massive ground ice body is relict glacier ice. Examination of data from a separate study on ice crystallography and isotopic composition of the massive ice body does not contradict this conclusion. The isotope composition and profiles conform with what can be expected for deformed basal ice. The chronology for the Shpindler Cape sequence implies that the glacier ice might be older than 250\hspace{0.167em}ka years. Consequently, permafrost has preserved the relict glacier ice for the duration of at least two interglacials (Eemian and Holocene), as well as several Saalian and Weichselian interstadials, illustrating the preservation potential of the permafrost. Copyright {\textcopyright} 2003 John Wiley {\&} Sons, Ltd.},
  doi          = {10.1002/ppp.455},
  isbn         = {1099-1530},
  journaltitle = {Permafrost and Periglacial Processes},
  keywords     = {Arctic Russia,Glacier ice,Massive ground ice,Permafrost},
}

@Article{Ingolfsson2008,
  author       = {Ing{\'{o}}lfsson, {\'{O}}lafur and M{\"{o}}ller, Per and Lokrantz, Hanna},
  title        = {{Late Quaternary marine-based Kara Sea ice sheets: A review of terrestrial stratigraphic data highlighting their formation}},
  year         = {2008},
  volume       = {27},
  number       = {2},
  pages        = {152--161},
  issn         = {0800-0395},
  abstract     = {Reconstructions of the Late Quaternary glacial history of the Kara Sea$\backslash$narea show repeated build-up of ice-sheet domes over the shallow$\backslash$nepicontinental Kara Sea. Inferred ice divides were situated over the$\backslash$ncentral Kara Sea, and the ice sheet repeatedly inundated the surrounding$\backslash$ncoastal areas of western Siberia. Geological fingerprinting of the Kara$\backslash$nSea ice sheet include end moraine zones, raised beaches, tills,$\backslash$nglaciotectonic deformations and coarsening-upward sediment sequences,$\backslash$nreflecting isostatic rebound cycles. This paper reviews evidence from$\backslash$nseveral areas along the perimeter of the Kara Sea, suggesting that$\backslash$nperipheral sites were critical for the initiation of the large Kara Sea$\backslash$nice sheet. Ice-sheet inception progressed with the formation of local$\backslash$nice caps that later coalesced on the adjacent shelf with globally$\backslash$nfalling sea levels, eventually merging and growing into a large ice$\backslash$ndome.},
  doi          = {10.1111/j.1751-8369.2008.00060.x},
  isbn         = {$08000395\backslash$n17518369},
  journaltitle = {Polar Research},
  keywords     = {Kara Sea,Late Quaternary,Marine-basedice sheet,Western Siberia},
}

