@Article{Thomsen1990,
  author       = {Thomsen, Leon A.},
  title        = {{Poisson was not a geophysicist!}},
  year         = {1990},
  volume       = {9},
  number       = {12},
  pages        = {27--29},
  issn         = {1070-485X},
  abstract     = {The purpose of this Round Table essay is to argue that Poisson's ratio is not relevant to any problem in seismology, and that we should clear our minds of it, and of the confusion that it entrains. The reason for its irrelevance is that Poisson's ratio is defined in terms of an experiment (axial compression or tension of a thin bar) which is not relevant to wave propagation (although it may be a common situation in engineering mechanics). I then extend the argument (with less certitude) to argue that Poisson's ratio is not useful anywhere in exploration geophysics.},
  doi          = {10.1190/1.1439706},
  file         = {Thomsen1990 - Poisson was not a geophysicist!.pdf:Thomsen1990 - Poisson was not a geophysicist!.pdf:PDF},
