@Article{Csatho2005,
  author       = {Csatho, Bea M and {Van Der Veen}, Cornelis J and Tremper, Catherine M},
  title        = {{Trimline Mapping from Multispectral Landsat ETM+ Imagery}},
  year         = {2005},
  volume       = {59},
  number       = {1},
  pages        = {49--62},
  issn         = {0705-7199},
  abstract     = {La zone proglaciaire de la r{\~{A}}{\textcopyright}gion de Jakobshavn Isfjorf, au Groenland occidental, est {\~{A}}{\textcopyright}tudi{\~{A}}{\textcopyright}e par l'imagerie multispectrale Landsat ETM+, avec un accent sur la limite atteinte par les glaces durant le Petit {\~{A}}?ge Glaciaire. L'extension maximale des glaces est reconstitu{\~{A}}{\textcopyright}e par t{\~{A}}{\textcopyright}l{\~{A}}{\textcopyright}d{\~{A}}{\textcopyright}tection satellitaire et compar{\~{A}}{\textcopyright}e aux donn{\~{A}}{\textcopyright}es cartographiques bas{\~{A}}{\textcopyright}es sur la photogramm{\~{A}}{\textcopyright}trie et des mesures de terrain. Une classification dirig{\~{A}}{\textcopyright}e a permis d'identifier 14{\^{A}} types de surfaces allant de la neige et de la glace, avec ou sans d{\~{A}}{\textcopyright}bris en surface, {\~{A}} divers types de couverture v{\~{A}}{\textcopyright}g{\~{A}}{\textcopyright}tale, en passant par divers degr{\~{A}}{\textcopyright}s de turbidit{\~{A}}{\textcopyright} de l'eau. Une carte morphologique digitale de synth{\~{A}}{\"{}}se, avec cinq classes distinctes, est produite pour d{\~{A}}{\textcopyright}terminer la justesse de la classification. La pr{\~{A}}{\textcopyright}cision de l'emplacement de la est valid{\~{A}}{\textcopyright}e par des mesures au GPS le long du versant nord du fjord de Jakobshavn. Apr{\~{A}}{\"{}}s la fusion de trois bandes spectrales avec la bande panchromatique, une image avec une r{\~{A}}{\textcopyright}solution spatiale de 15{\^{A}} m montre clairement la morphologie des glaces, avec une finesse accrue de la g{\~{A}}{\textcopyright}omorphologie glaciaire dans la zone marginale attenante.Multispectral Landsat ETM+{\^{A}} imagery is used to study the ice-marginal region in the vicinity of Jakobshavn Isfjord, west Greenland. In particular, the trimline indicating margin retreat since the maximum stand attained during the Little Ice Age maximum is reconstructed, and compared with earlier maps based on aerial photogrammetry and ground surveys. Applying supervised classification, fourteen different surface types were identified, ranging from snow and ice, debris-covered ice and water with differing turbidities, to different types of vegetative landcover. After similar classes were merged into five, distinctively different classes, a digitized geomorphologic map was used to assess the accuracy of the classification. The positional accuracy of the trimline was checked by using results from a GPS survey along northern slope of the Jakobshavn fjord. By merging three spectral bands with the panchromatic band, a pan-sharpened image with a spatial resolution of 15{\^{A}} m is obtained that clearly shows morphological features on the ice surface, as well as increased resolution of glacial geomorphology.},
  doi          = {10.7202/013736ar},
  file         = {Csatho2005 - Trimline Mapping from Multispectral Landsat ETM+ Imagery.pdf:Csatho2005 - Trimline Mapping from Multispectral Landsat ETM+ Imagery.pdf:PDF;Csatho2005 - Trimline Mapping from Multispectral Landsat ETM+ Imagery.pdf:pdf2/Csatho, Van Der Veen, Tremper/Csatho2005 - Trimline Mapping from Multispectral Landsat ETM+ Imagery.pdf:PDF},
  journaltitle = {G{\~{A}}{\textcopyright}ographie physique et Quaternaire},
}

