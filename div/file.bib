@Article{Appleby2004,
  author       = {Appleby, P. G.},
  title        = {{Environmental change and atmospheric contamination on {Svalbard}: Sediment chronology}},
  year         = {2004},
  volume       = {31},
  number       = {4},
  pages        = {433--443},
  issn         = {0921-2728},
  abstract     = {Sediment cores collected from eight lakes along the western coast of Svalbard as part of a project investigating atmospheric pollution and environmental change in Arctic regions were dated radiometrically using natural (210Pb) and artificial (137Cs and 241Am) fallout radionuclides. At four sites the sedimentation rates were relatively uniform, and in consequence the 210Pb dates were relatively unambiguous. At the remaining sites there were irregularities in the 210Pb activity versus depth profiles, indicating significant variations in the net sediment accumulation rate during the past 120 years. At these sites, there were significant differences between 210Pb dates calculated using the two standard simple dating models, constant rate of supply (CRS) and constant initial concentration (CIC). In most cases, stratigraphic dates based on the 137Cs and 241Am records supported use of the CRS model, though at one site (Ossian Sarsfjellet) the CIC model appeared more appropriate. The irregularities in the 210Pb records were mainly caused by episodes of accelerated sedimentation due, for example, to inwash or slump events, though at some sites there appears to have been a systematic increase in sediment accumulation rates in recent decades. Sediment accumulation rates were generally lower at the northern sites, and higher at the more southerly locations. Mean sediment accumulation rates varied by an order of magnitude, ranging from 0.002 to 0.050 g cm$-$2 y$-$1 (0.02--0.10 cm y$-$1). 210Pb fluxes measured from the core inventories were mostly in the range 34--80 Bq m$-$2 y$-$1 typical of Arctic sites. Much higher values, recorded at two sites (Birgervatnet and Daltj{\o}rna), may be due to significant inputs from the catchments during spring thaw.},
  doi          = {10.1023/B:JOPL.0000022545.73163.ed},
  file         = {Appleby2004 - Environmental change and atmospheric contamination on Svalbard_ Sediment chronology.pdf:pdf2/Appleby/Appleby2004 - Environmental change and atmospheric contamination on Svalbard_ Sediment chronology.pdf:PDF;Appleby2004 - Environmental change and atmospheric contamination on Svalbard_ Sediment chronology.pdf:Appleby2004 - Environmental change and atmospheric contamination on Svalbard_ Sediment chronology.pdf:PDF},
  isbn         = {1573-0417},
  journaltitle = {Journal of Paleolimnology},
  keywords     = {137Cs,210Pb,241Am,Lake sediments,Radiometric dating,Sediment chronology,Svalbard},
}

@Article{Arevalo2009,
  author       = {Arevalo, Ricardo and McDonough, William F. and Luong, Mario},
  title        = {{The K/U ratio of the silicate Earth: Insights into mantle composition, structure and thermal evolution}},
  year         = {2009},
  volume       = {278},
  number       = {3-4},
  pages        = {361--369},
  issn         = {0012-821X},
  abstract     = {The abundance of K in the silicate Earth provides control on the composition of the Earth's interior, the dominant mode of mantle convection, the thermal evolution of the planet, and the concentration of Pb in the core. Because K acts as a volatile species during accretion, the K content of the silicate Earth is determined as a function of the terrestrial K/U ratio. A comprehensive examination of MORB from the Atlantic, Indian and Pacific oceans, including both normal- and enriched-type samples, reveals a composite MORB source K/U ratio of 19,000 ?? 2600 (2??). In comparison, ocean island basalts and average continental crust have average K/U values of 11,900 ?? 2200 and 13,000 ?? 3000, respectively. The fractional contributions of these reservoirs establishes the K/U ratio of the silicate Earth to be 13,800 ?? 2600 (2??), equating to 280 ?? 120????g/g K in the silicate Earth. As a result, the planet's convective Urey ratio is verified to be {\~{}} 0.34, which indicates a current mantle cooling rate of 70-130??K Gyr- 1 after taking into account potential heat flux across the core-mantle boundary. Additionally, the Earth's balance of radiogenic heat flow and budget of 40Ar necessitate a lower mantle reservoir enriched in radioactive elements. The bulk Earth Pb/U ratio, determined here to be {\~{}} 85, suggests {\~{}} 1200??ng/g Pb in the bulk Earth and ??? 3300??ng/g Pb in the core. ?? 2008 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.epsl.2008.12.023},
  file         = {Arevalo2009 - The K_U ratio of the silicate Earth_ Insights into mantle composition, structure and thermal evolution.pdf:Arevalo2009 - The K_U ratio of the silicate Earth_ Insights into mantle composition, structure and thermal evolution.pdf:PDF;Arevalo2009 - The K_U ratio of the silicate Earth_ Insights into mantle composition, structure and thermal evolution.pdf:pdf2/Arevalo, McDonough, Luong/Arevalo2009 - The K_U ratio of the silicate Earth_ Insights into mantle composition, structure and thermal evolution.pdf:PDF},
  isbn         = {0012-821X},
  journaltitle = {Earth and Planetary Science Letters},
  keywords     = {argon,degas,potassium,radiogenic,uranium,volatile},
  pmid         = {264046900021},
  publisher    = {Elsevier B.V.},
}

@Article{Argus2011,
  author       = {Argus, Donald F. and Blewitt, Geoffrey and Peltier, W. Richard and Kreemer, Corn{\'{e}}},
  title        = {{Rise of the Ellsworth mountains and parts of the East Antarctic coast observed with GPS}},
  year         = {2011},
  volume       = {38},
  number       = {16},
  pages        = {3--7},
  issn         = {0094-8276},
  abstract     = {Using GPS observations from 1996 to 2011, we constrain postglacial$\backslash$nrebound in Antarctica. Sites in the Ellsworth mountains, West Antarctica,$\backslash$nare rising at {\&}{\#}8776;5 �4 mm/yr (95{\%} confidence limits), as in the$\backslash$npostglacial rebound model of Peltier, but {\&}{\#}8776;10 mm/yr slower$\backslash$nthan in the model of Ivins and James. Therefore significant ice loss$\backslash$nfrom the Ellsworth mountains ended by 4 ka, and current ice loss$\backslash$nthere is less than inferred from GRACE gravity observations in studies$\backslash$nassuming the model of Ivins and James. Three sites along the coast$\backslash$nof East Antarctica are rising at 3 to 4 �2 mm/yr, in viscous response$\backslash$nto Holocene unloading of ice along the Queen Maud Land coast and$\backslash$nelsewhere. Kerguelen island and seven sites along the coast of East$\backslash$nAntarctic are part of a rigid Antarctica plate. O'Higgins, northern$\backslash$nAntarctic peninsula, is moving southeast at 2.3 �0.6 mm/yr relative$\backslash$nto the Antarctic plate.},
  doi          = {10.1029/2011GL048025},
  file         = {Argus2011 - Rise of the Ellsworth mountains and parts of the East Antarctic coast observed with GPS.pdf:pdf2/Argus et al/Argus2011 - Rise of the Ellsworth mountains and parts of the East Antarctic coast observed with GPS.pdf:PDF;Argus2011 - Rise of the Ellsworth mountains and parts of the East Antarctic coast observed with GPS.pdf:Argus2011 - Rise of the Ellsworth mountains and parts of the East Antarctic coast observed with GPS.pdf:PDF},
  journaltitle = {Geophysical Research Letters},
}

