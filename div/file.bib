}

@Article{Steig1998a,
  author       = {Steig, Eric J. and Wolfe, Alexander P. and Miller, Gifford H.},
  title        = {{Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic {Canada}: Coupled evidence from cosmogenic isotopes and lake sediments}},
  year         = {1998},
  volume       = {26},
  number       = {9},
  pages        = {835--838},
  issn         = {0091-7613},
  abstract     = {New data constrain the timing and character of glaciation of eastern Baffin Island, Arctic Canada. Lake sediments radiocarbon dated to between 14 and {\textgreater}52 ka, coupled to 10Be and 26Al exposure ages {\textgreater}35 ka from adjacent moraines and bedrock, demonstrate that some coastal uplands remained above the limits of continental glaciation throughout oxygen isotope stage 2. At lower elevations, exposure ages from a flight of distinct lateral moraines range from 10 to 35 ka, and imply that low-gradient outlet glaciers reached the outer coast during the late Wisconsinan. Collectively, these results reconcile terrestrial evidence for the presence of ice-free refugia through the late Wisconsinan with marine evidence for extensive outlet glaciers in the major fiords and bays of eastern Baffin Island.},
  doi          = {10.1130/0091-7613(1998)026<0835:WRATGH>2.3.CO;2},
  file         = {Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:pdf2/Steig, Wolfe, Miller/Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:PDF;Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:PDF},
  isbn         = {0091-7613},
  journaltitle = {Geology},
  pmid         = {905},
}

