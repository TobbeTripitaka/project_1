@Article{Cande2011,
  author       = {Cande, Steven C and Stegman, Dave R},
  title        = {{Indian and African plate motions driven by the push force of the R{\'{e}}union plume head.}},
  year         = {2011},
  volume       = {475},
  number       = {7354},
  pages        = {47--52},
  issn         = {0028-0836},
  abstract     = {Mantle plumes are thought to play an important part in the Earth's tectonics, yet it has been difficult to isolate the effect that plumes have on plate motions. Here we analyse the plate motions involved in two apparently disparate events--the unusually rapid motion of India between 67 and 52 million years ago and a contemporaneous, transitory slowing of Africa's motion--and show that the events are coupled, with the common element being the position of the Indian and African plates relative to the location of the R{\'{e}}union plume head. The synchroneity of these events suggests that they were both driven by the force of the R{\'{e}}union plume head. The recognition of this plume force has substantial tectonic implications: the speed-up and slowdown of India, the possible cessation of convergence between Africa and Eurasia in the Palaeocene epoch and the enigmatic bends of the fracture zones on the Southwest Indian Ridge can all be attributed to the R{\'{e}}union plume.},
  doi          = {10.1038/nature10174},
  file         = {Cande2011 - Indian and African plate motions driven by the push force of the R_'eunion plume head..pdf:Cande2011 - Indian and African plate motions driven by the push force of the R_'eunion plume head..pdf:PDF},
  isbn         = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
  journaltitle = {Nature},
  pmid         = {21734702},
  publisher    = {Nature Publishing Group},
}

