  pages        = {1--5},
  issn         = {0094-8276},
  abstract     = {We analyze data from continuous GPS stations in the southwestern United States and present the fi r s t observations of a systematic motion of the Colorado Plateau region. Relative to a North Americafixed reference frame, westward velocities increase rapidly across the Wasatch fault system, which comprises the plateaus northwestern margin, but increase more gradually between the Great Plains and the Mojave Desert province. We detect no significant extension across most of the Rio Grande Rift, but extension could be as high as 0.5 mm yr1 along its southernmost portion. We suggest that the motion between the plateau and stable continent may partly be accommodated instead across the Jemez Lineament. Only when we consider GPS velocities within the plateaus center, do the data support a rigid body rotation of 0.103 Ma1 around a pole in the northern Rocky Mountains. We conclude that active extension has encroached into the plateau from both the east and west.},
  doi          = {10.1029/2010GL043374},
  file         = {Kreemer2010 - Present-day motion and deformation of the Colorado Plateau.pdf:Kreemer2010 - Present-day motion and deformation of the Colorado Plateau.pdf:PDF;Kreemer2010 - Present-day motion and deformation of the Colorado Plateau.pdf:pdf2/Kreemer, Blewitt, Bennett/Kreemer2010 - Present-day motion and deformation of the Colorado Plateau.pdf:PDF;Kreemer2010a - Supplemental Material.pdf:pdf2/Kreemer/Kreemer2010a - Supplemental Material.pdf:PDF;Kreemer2010a - Supplemental Material.pdf:Kreemer2010a - Supplemental Material.pdf:PDF},
  isbn         = {0094-8276},
  journaltitle = {Geophysical Research Letters},
}

@Article{Kreemer2014,
  author       = {Kreemer, Corn{\'{e}} and Gordon, Richard G.},
  title        = {{Pacific plate deformation from horizontal thermal contraction}},
  year         = {2014},
  volume       = {42},
  number       = {10},
  pages        = {847--850},
  issn         = {1943-2682},
  abstract     = {The central approximation of plate tectonics is that the plates are rigid, which gives the$\backslash$ntheory its rigor and predictive power. Space geodetic measurements are consistent with the$\backslash$nrigidity of stable plate interiors, but some failures of plate-circuit closure, in particular of$\backslash$noceanic plates, indicate that plates may be measurably non-rigid. We explore the hypothesis$\backslash$nthat horizontal thermal contraction causes deformation of oceanic plates. Here we show$\backslash$nsignificant expected displacement fields due to thermal contraction for the Pacific plate$\backslash$nbased on a previously proposed relationship between seafloor age and strain rate and on$\backslash$ntwo end-member assumptions on how strain compatibility is enforced. The predicted maximum$\backslash$n2.2 mm/yr southeastward motion of the northeastern part of the plate relative to the$\backslash$nPacific-Antarctic Rise may contribute to a large part of the non-closure of the Pacific--North$\backslash$nAmerica plate motion circuit. Our predicted displacement rates cannot (yet) be confirmed$\backslash$nby current space geodetic data and will require seafloor geodesy with 1 mm/yr accuracy. The$\backslash$nspatial distribution of predicted moment rate agrees reasonably well with that of intraplate$\backslash$nearthquake epicenters, similar to what is observed for plate boundary zones. Our results$\backslash$nsuggest that plate-scale horizontal thermal contraction is significant, and that it may be$\backslash$npartly released seismically.},
  doi          = {10.1130/G35874.1},
  file         = {Kreemer2014 - Pacific plate deformation from horizontal thermal contraction.pdf:Kreemer2014 - Pacific plate deformation from horizontal thermal contraction.pdf:PDF;Kreemer2014 - Pacific plate deformation from horizontal thermal contraction.pdf:pdf2/Kreemer, Gordon/Kreemer2014 - Pacific plate deformation from horizontal thermal contraction.pdf:PDF},
  journaltitle = {Geology},
}

