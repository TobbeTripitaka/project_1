@Article{Bondevik2006,
  author       = {Bondevik, Stein and Mangerud, Jan. and Birks, Hilary H and Gulliksen, Steinar and Reimer, Paula},
  title        = {{Changes in north Atlantic Radiocarbon Reservoir Ages During the Allerod and Younger Dryas}},
  year         = {2006},
  volume       = {312},
  number       = {5779},
  pages        = {1514--1517},
  issn         = {1095-9203},
  abstract     = {Estimates of the radiocarbon age of seawater are required in correlations between marine and terrestrial records of the late Quaternary climate. We radiocarbon-dated marine shells and terrestrial plant remains deposited in two bays on Norway's west coast between 11,000 and 14,000 years ago, a time of large and abrupt climatic changes that included the Younger Dryas (YD) cold episode. The radiocarbon age difference between the shells and the plants showed that sea surface reservoir ages increased from 400 to 600 years in the early YD, stabilized for 900 years, and dropped by 300 years within a century across the YD-Holocene transition.},
  doi          = {10.1126/science.1123300},
