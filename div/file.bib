@Article{Grafenstein1998,
  author       = {von Grafenstein, U. and Erlenkeuser, H. and M{\&}{\#}x000FC;ller, J. and Jouzel, J. and Johnsen, S.},
  title        = {{The cold event 8200 years ago documented in oxygen isotope records of precipitation in {Europe} and Greenland}},
  year         = {1998},
  volume       = {14},
  number       = {2},
  pages        = {73--81},
  issn         = {0930-7575},
  abstract     = {Stable oxygen isotope ratios of ostracod valves in Late Glacial and Holocene sediments of core AS 92-5 from deep lake Ammersee (southern Germany) reflect variations of mean oxygen isotope ratios in past atmospheric precipitation. The record reconfirms the strong similarity of climate evolution in Europe and Greenland during the last deglaciation. For the first time in Europe, we find a 200-year-long negative ? {\#}O- excursion, which is contemporaneous with the stron- gest negative ? {\#}O-excursion in the Greenland ice around 8.2 ky before present. The 8.2 ky isotopic event on both sides of the North Atlantic ocean is interpreted as a cold period, most probably induced by a perturba- tion of the North Atlantic thermohaline circulation. We discuss two possible triggering mechanisms: (1) weak forcing (as proposed by Alley et al.), and (2) forcing by a strong and sudden freshwater pulse from the collapse of the Hudson Ice Dome.},
  doi          = {10.1007/s003820050210},
  file         = {Grafenstein1998 - The cold event 8200 years ago documented in oxygen isotope records of precipitation in Europe and Greenland.pdf:pdf2/von Grafenstein et al/Grafenstein1998 - The cold event 8200 years ago documented in oxygen isotope records of precipitation in Europe and Greenland.pdf:PDF;Grafenstein1998 - The cold event 8200 years ago documented in oxygen isotope records of precipitation in Europe and Greenland.pdf:Grafenstein1998 - The cold event 8200 years ago documented in oxygen isotope records of precipitation in Europe and Greenland.pdf:PDF},
