@Article{Pollard2005,
  author       = {Pollard, David and DeConto, Robert M. and Nyblade, Andrew A.},
  title        = {{Sensitivity of Cenozoic Antarctic ice sheet variations to geothermal heat flux}},
  year         = {2005},
  volume       = {49},
  number       = {1-2},
  pages        = {63--74},
  issn         = {0921-8181},
  abstract     = {The sensitivity of long-term Cenozoic variations of the East Antarctic ice sheet to geothermal heat flux is investigated, using a coupled climate-ice sheet model with various prescribed values and patterns of geothermal heat flux. The sudden growth of major ice across the Eocene-Oligocene boundary ($\sim$34 Ma) is used as a test bed for this sensitivity. A suite of several million year-long simulations spanning the transition is performed, with various geothermal heat flux magnitudes and spatial distributions reflecting current uncertainty. The climate-ice sheet model simulates the Eocene-Oligocene transition realistically as a non-linear ice-sheet response to orbital perturbations and a long-term gradual decline of atmospheric CO2. It is found that reasonable variations of geothermal heat flux have very little effect on overall ice volumes and extents, and on the timing of major ice transitions. However, they cause large changes in basal areas at the pressure melting point at a given time, which could strongly influence other aspects of Cenozoic Antarctic evolution such as basal hydrology, sediment deformation and discharge, subglacial lakes, and basal erosional forms. {\textcopyright} 2005 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.gloplacha.2005.05.003},
  file         = {Pollard2005 - Sensitivity of Cenozoic Antarctic ice sheet variations to geothermal heat flux.pdf:pdf2/Pollard, DeConto, Nyblade/Pollard2005 - Sensitivity of Cenozoic Antarctic ice sheet variations to geothermal heat flux.pdf:PDF;Pollard2005 - Sensitivity of Cenozoic Antarctic ice sheet variations to geothermal heat flux.pdf:Pollard2005 - Sensitivity of Cenozoic Antarctic ice sheet variations to geothermal heat flux.pdf:PDF},
  isbn         = {0921-8181},
  journaltitle = {Global and Planetary Change},
  keywords     = {Antarctica,Cenozoic,Geothermal heat flow,Ice sheets},
  pmid         = {2006049014},
}

