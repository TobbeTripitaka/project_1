@InProceedings{Bodin2009,
  author        = {Bodin, Thomas and Sambridge, Malcolm},
  title         = {{Seismic tomography with the reversible jump algorithm}},
  booktitle     = {Geophysical Journal International},
  year          = {2009},
  volume        = {178},
  number        = {3},
  pages         = {1411--1436},
  __markedentry = {[tobiasstal:1]},
  abstract      = {Many geophysical data, such as the frequencies of the free oscillations$\backslash$nof the Earth, depend on more than one type of model parameter. For$\backslash$ninverse problems depending on multiple parameter classes, an iterative$\backslash$nsolution procedure is introduced in which each parameter class can$\backslash$nbe treated in the same way. This approach has considerable advantages$\backslash$nwhere a large number of parameters are employed, but can still be$\backslash$nuseful for smaller systems. The iteration by parameter class commences$\backslash$nby solving for the direct dependence on a particular parameter class,$\backslash$nand at subsequent iterations the cross-dependences between classes$\backslash$nare introduced. The update affects only the right-hand side of the$\backslash$nequations, and because the same sets of equations have to be sloved$\backslash$nat each iteration, an efficient computational implementation can$\backslash$nbe made. The largest set of equations that has to be solved at a$\backslash$ntime corresponds to the number of variables in an individual parameter$\backslash$nclass rather than the full set of parameters, which confers substantial$\backslash$ncomputational benefits for very large problems.},
  doi           = {10.1111/j.1365-246X.2009.04226.x},
  file          = {Bodin2009 - Seismic tomography with the reversible jump algorithm.pdf:Bodin2009 - Seismic tomography with the reversible jump algorithm.pdf:PDF},
  isbn          = {0956540X$\backslash$r1365246X},
  issn          = {0956-540X},
  keywords      = {Australia,Computational seismology,Inverse theory,Probability distribution,Seismic tomography,Tomography},
}

