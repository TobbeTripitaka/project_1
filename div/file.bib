@Article{Kaplan2001,
  author       = {Kaplan, M. R. and Miller, G. H. and Steig, E. J.},
  title        = {{Low-gradient outlet glaciers (ice streams?) drained the Laurentide ice sheet}},
  year         = {2001},
  volume       = {29},
  number       = {4},
  pages        = {343--346},
  issn         = {0091-7613},
  abstract     = {Observations of geomorphic features dated with cosmogenic nuclides are$\backslash$nused to determine the configuration of glaciation in Cumberland Sound,$\backslash$nBaffin Island, during the last glacial maximum. Cumberland Sound, which$\backslash$nwas a major drainage of the Laurentide Ice Sheet at its northeastern$\backslash$n(Arctic Canada) margin, contained low-gradient ice with a surface slope$\backslash$nof similar to0.03 degrees and a driving stress between 2 and 7 kPa.$\backslash$nThese values are similar to those observed for the ice streams that$\backslash$ndominate the dynamics of the West Antarctic ice sheet today. The$\backslash$npresence of low-gradient ice in Cumberland Sound and other major outlets$\backslash$nmay similarly have influenced Laurentide ice sheet behavior and would$\backslash$naccount for the iceberg discharge episodes that produced the$\backslash$nHeinrich-event debris layers observed in North Atlantic ocean sediments.},
  doi          = {10.1130/0091-7613(2001)029<0343:LGOGIS>2.0.CO;2},
  file         = {Kaplan2001 - Low-gradient outlet glaciers (ice streams_) drained the Laurentide ice sheet.pdf:Kaplan2001 - Low-gradient outlet glaciers (ice streams_) drained the Laurentide ice sheet.pdf:PDF;Kaplan2001 - Low-gradient outlet glaciers (ice streams_) drained the Laurentide ice sheet.pdf:pdf2/Kaplan, Miller, Steig/Kaplan2001 - Low-gradient outlet glaciers (ice streams_) drained the Laurentide ice sheet.pdf:PDF},
  journaltitle = {Geology},
  keywords     = {Baffin Island,Exposure age,Glacial geology,Ice streams,Last glacial maximum,Laurentide ice sheet},
  pmid         = {2458420},
}

