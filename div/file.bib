@Article{Blaauw2007,
  author       = {Blaauw, M and Christen, J A and Mauquoy, D and {Van Der Plicht}, J and Bennett, Keith D},
  title        = {{Testing the timing of radiocarbon-dated events between proxy archives}},
  year         = {2007},
  volume       = {2},
  number       = {17},
  pages        = {283--288},
  issn         = {0959-6836},
  abstract     = {For interpreting past changes on a regional or global scale, the timings of proxy-inferred events are usually aligned with data from other locations. However, too often chronological uncertainties are ignored in proxy diagrams and multisite comparisons, making it possible for researchers to fall into the trap of sucking separate events into one illusionary event (or vice versa). Here we largely solve this 'suck in and smear syndrome' for radiocarbon (14 C) dated sequences. In a Bayesian framework, millions of plausible age-models are constructed to quantify the chronological uncertainties within and between proxy archives. We test the technique on replicated high-resolution 14 C-dated peat cores deposited during the 'Little Ice Age' (c. AD 1400 {\'{A}}1900), a period characterized by abrupt climate changes and severe 14 C calibration problems. Owing to internal variability in proxy data and uncertainties in age-models, these (and possibly many more) archives are not consistent in recording decadal climate change. Through explicit statistical tests of palaeoenvironmental hypotheses, we can move forward to systematic interpretations of proxy data. However, chronological uncertainties of non-annually resolved palaeoclimate records are too large for answering decadal timescale questions.},
  doi          = {10.1177/0959683607075857},
  file         = {Blaauw2007 - Testing the timing of radiocarbon-dated events between proxy archives.pdf:pdf2/Blaauw et al/Blaauw2007 - Testing the timing of radiocarbon-dated events between proxy archives.pdf:PDF;Blaauw2007 - Testing the timing of radiocarbon-dated events between proxy archives.pdf:Blaauw2007 - Testing the timing of radiocarbon-dated events between proxy archives.pdf:PDF},
  isbn         = {0959683607},
  journaltitle = {The Holocene},
  keywords     = {'Little Ice Age',Bayesian age-modelling,chronological uncertainty,meta-analysis,radiocarbon,synchronicity of events},
  pmid         = {220549194},
}

@Article{Blaauw2004,
  author       = {Blaauw, Maarten and van Geel, Bas and Mauquoy, Dmitri and van der Plicht, Johannes},
  title        = {{Carbon-14 wiggle-match dating of peat deposits: Advantages and limitations}},
  year         = {2004},
  volume       = {19},
  number       = {2},
  pages        = {177--181},
  issn         = {0267-8179},
  abstract     = {Carbon-14 wiggle-match dating (WMD) of peat deposits uses the non-linear relationship between 14C age and calendar age to match the shape of a series of closely spaced peat 14C dates with the 14C calibration curve. The method of WMD is discussed, and its advantages and limitations are compared with calibration of individual dates. A numerical approach to WMD is introduced that makes it possible to assess the precision of WMD chronologies. During several intervals of the Holocene, the 14C calibration curve shows less pronounced fluctuations. We assess whether wigglematching is also a feasible strategy for these parts of the 14C calibration curve. High-precision chronologies, such as obtainable with WMD, are needed for studies of rapid climate changes and their possible causes during the Holocene},
  doi          = {10.1002/jqs.810},
  file         = {Blaauw2004 - Carbon-14 wiggle-match dating of peat deposits_ Advantages and limitations.pdf:pdf2/Blaauw et al/Blaauw2004 - Carbon-14 wiggle-match dating of peat deposits_ Advantages and limitations.pdf:PDF;Blaauw2004 - Carbon-14 wiggle-match dating of peat deposits_ Advantages and limitations.pdf:Blaauw2004 - Carbon-14 wiggle-match dating of peat deposits_ Advantages and limitations.pdf:PDF},
  isbn         = {0267-8179},
  journaltitle = {Journal of Quaternary Science},
  keywords     = {14C calibration,14C chronologies,14C wiggle-match dating},
}

