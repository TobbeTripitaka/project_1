@Article{ArroyoFranco2006,
  author       = {{Arroyo Franco}, J L and {Mercado Ortiz}, M A and {De Gopa}, S and Renlie, L and Williams, S},
  title        = {{Sonic Investigations In and Around the Borehole}},
  year         = {2006},
  volume       = {18},
  number       = {1},
