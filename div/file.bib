@Article{Linge2007,
  author       = {Linge, Henriette and Olsen, Lars and Brook, Edward J. and Darter, Jessica R. and Mickelson, David M. and Raisbeck, Grant M. and Yiou, Fran{\c{c}}oise},
  title        = {{Cosmogenic nuclide surface exposure ages from Nordland, northern {Norway}: Implications for deglaciation in a coast to inland transect}},
  year         = {2007},
  volume       = {87},
  number       = {1-2},
  pages        = {269--280},
  issn         = {0029-196X},
  abstract     = {The time and style of deglaciation has been investigated by analysis of in situ Be-10 in bedrock samples from three sites in Nordland, northern Norway. At the coast, Be-10 data indicate rapid deglaciation of the island (A) over circle moya at about 14.3 0.4 Be-10 ka (n=9). In the ford area, perched blocks and erratic boulders indicate that the summit of Helgelandsbukken was ice covered at some stage, yet little affected by glacial erosion as is evident from the Be-10 data ({\textgreater}30 Be-10 ka, n=5). Further inland, Be-10 data from Addjektind indicate that the Fennoscandian ice sheet was fragmented into valley glaciers by c. 10.2 0.3 Be-10 ka (n=7). Preliminary 2D ice-flow modelling supports the degree of glacial erosion as inferred from the Be-10 data; past ice cover has been 'thick and erosive' at the coast and partly in the inland, whereas 'thin and preserving' and probably cold-based at summits in the fjord zone. The Be-10 ages presented here support and add detail to the regional C-14-based deglaciation chronology. Tentative rates of ice-surface lowering are {\textgreater}25 cm yr(-1) at the coast (18-14 ka) and similar to 5 cm yr(-1) inland (18-10 ka).},
  file         = {Linge2007 - Cosmogenic nuclide surface exposure ages from Nordland, northern Norway_ Implications for deglaciation in a coast to inland transect.pdf:pdf2/Linge et al/Linge2007 - Cosmogenic nuclide surface exposure ages from Nordland, northern Norway_ Implications for deglaciation in a coast to inland transect.pdf:PDF;Linge2007 - Cosmogenic nuclide surface exposure ages from Nordland, northern Norway_ Implications for deglaciation in a coast to inland transect.pdf:Linge2007 - Cosmogenic nuclide surface exposure ages from Nordland, northern Norway_ Implications for deglaciation in a coast to inland transect.pdf:PDF},
  isbn         = {0029-196X},
  journaltitle = {Norsk Geologisk Tidsskrift},
}

