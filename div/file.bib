@Article{Polom2010,
  author       = {Polom, U and Hansen, L and Sauvin, G and L'Heureux, Jean-S{\'{e}}bastien and Lecomte, I and Krawczyk, C M and Vanneste, M and Longva, O},
  title        = {{High-resolution {\{}SH{\}}-wave seismic reflection for characterization of onshore ground conditions in the Trondheim Harbor, Central Norway}},
  year         = {2010},
  pages        = {297--312},
  abstract     = {The area around Trondheim Bay in central Norway is affected by landslides, both onshore and within the fjord, with several events documented to have occurred in the last century. As urban development, including land reclamation, is taking place in the harbor, assessing in situ soil conditions is paramount for infrastructure and operational safety. To obtain better insight into the harbor setting in terms of subsurface structures and potential coastal geo-hazards, a high-resolution multichannel {\{}SH{\}}-wave seismic-reflection land survey was carried out during summer 2008, which complements a dense network of high-resolution, single-channel marine seismic profiles over the deltaic sediments in the fjord. The {\{}SH{\}}-wave seismic reflection was chosen because the resulting interval shear-wave velocity provides a nearly direct proxy for in situ soil stiffness, a key geotechnical parameter. In total, 4.2 km of 2.5D {\{}SH{\}}-wave profiles was acquired along roads and parking places. Highly resolved images of the sediments were obtained, overlying the bedrock at a depth of about 150 m. The high quality of the data is ascribed to the quieter ambient noise conditions of the nighttime data collection and an efficient suppression of Love waves arising from the presence of a high-velocity layer at the surface. Five main stratigraphic units were identified based on reflection patterns and amplitudes. Distinct {\{}SH{\}}-wave reflection events enabled detailed S-wave velocity determination down to the bedrock. Subsequently, interval velocities were remapped into soil stiffness. Low S-wave velocities of about 100 m/s occurring in the upper 50 m of the fjord-deltaic sediment succession suggest low sediment stiffness (50 to 100 {\{}MPa{\}}) directly below the stiffer man-made fill that is 10 to 15 m thick. The results indicate that {\{}SH{\}}-wave seismic reflection is well suited for urban ground investigation.},
  doi          = {10.1190/1.9781560802259.ch18},
  file         = {Polom2010 - High-resolution _SH_-wave seismic reflection for characterization of onshore ground conditions in the Trondheim Harbor, Central Norway.pdf:Polom2010 - High-resolution _SH_-wave seismic reflection for characterization of onshore ground conditions in the Trondheim Harbor, Central Norway.pdf:PDF;Polom2010 - High-resolution _SH_-wave seismic reflection for characterization of onshore ground conditions in the Trondheim Harbor, Central Norway.pdf:pdf2/Polom et al/Polom2010 - High-resolution _SH_-wave seismic reflection for characterization of onshore ground conditions in the Trondheim Harbor, Central Norway.pdf:PDF},
  isbn         = {978-1-56080-224-2},
  journaltitle = {Advances in Near-surface Seismology and Ground-penetrating Radar},
}

@Article{Polyak2001,
  author       = {Polyak, L and Edwards, M H and Coakley, B J and Jakobsson, M},
  title        = {{Ice shelves in the Pleistocene Arctic Ocean inferred from glaciogenic deep-sea bedforms.}},
  year         = {2001},
  volume       = {410},
  number       = {6827},
  pages        = {453--457},
  issn         = {0028-0836},
  abstract     = {It has been proposed that during Pleistocene glaciations, an ice cap of 1 kilometre or greater thickness covered the Arctic Ocean. This notion contrasts with the prevailing view that the Arctic Ocean was covered only by perennial sea ice with scattered icebergs. Detailed mapping of the ocean floor is the best means to resolve this issue. Although sea-floor imagery has been used to reconstruct the glacial history of the Antarctic shelf, little data have been collected in the Arctic Ocean because of operational constraints. The use of a geophysical mapping system during the submarine SCICEX expedition in 1999 provided the opportunity to perform such an investigation over a large portion of the Arctic Ocean. Here we analyse backscatter images and sub-bottom profiler records obtained during this expedition from depths as great as 1 kilometre. These records show multiple bedforms indicative of glacial scouring and moulding of sea floor, combined with large-scale erosion of submarine ridge crests. These distinct glaciogenic features demonstrate that immense, Antarctic-type ice shelves up to 1 kilometre thick and hundreds of kilometres long existed in the Arctic Ocean during Pleistocene glaciations.},
  doi          = {10.1038/35068536},
  file         = {Polyak2001 - Ice shelves in the Pleistocene Arctic Ocean inferred from glaciogenic deep-sea bedforms..pdf:Polyak2001 - Ice shelves in the Pleistocene Arctic Ocean inferred from glaciogenic deep-sea bedforms..pdf:PDF},
  isbn         = {0028-0836},
  journaltitle = {Nature},
  pmid         = {11260709},
}

@Article{Polyak2008,
  author       = {Polyak, Leonid and Niessen, Frank and Gataullin, Valery and Gainanov, Valery},
  title        = {{The eastern extent of the Barents-Kara ice sheet during the Last Glacial Maximum based on seismic-reflection data from the eastern Kara Sea}},
  year         = {2008},
  volume       = {27},
  number       = {2},
  pages        = {162--174},
  issn         = {0800-0395},
  abstract     = {We present sub-bottom profiling (sparker and Parasound) results from the$\backslash$neastern Kara Sea, on the Eurasian Arctic margin, which enable the$\backslash$nidentification of the Last Glacial Maximum (LGM) ice extent. The$\backslash$nanalysed profiles show that glacigenic diamicton is ubiquitous at the$\backslash$nseafloor, east of about 95 degrees E and 78 degrees N. The eastern$\backslash$nmargin of this diamicton is expressed in a conspicuous morainic ridge at$\backslash$nthe entrance to the Vilkitsky Strait, and to the south the diamicton$\backslash$nprojection aligns with the LGM limit mapped at the north-western Taymyr.$\backslash$nThe bottom of the Voronin Trough further north is also covered with$\backslash$ndiamicton and has numerous erosional bedforms, indicating a streamlined$\backslash$nflow of grounded ice along the trough. Accurate dating of the diamicton$\backslash$nis not attainable, but the correlation of pre-diamict sediments to$\backslash$nwell-dated sections in the Laptev Sea, and available C-14 ages from$\backslash$nsediments on top of the diamicton, indicate its LGM age. These results$\backslash$nsupport the palaeogeographic reconstruction that assumes the extension$\backslash$nof the LGM Barents-Kara ice sheet as far east as Taymyr. This$\backslash$nconfiguration implies that LGM ice blocked the drainage of the Ob and$\backslash$nYenisey rivers on the Kara shelf. This inference is consistent with the$\backslash$npresence of large ({\textgreater} 100 km wide) lenses of basin infill adjacent to the$\backslash$nsouthern margin of the diamicton. However, the limited distribution of$\backslash$nthe eastern Kara ice lobe, not extending on Severnaya Zemlya, suggests$\backslash$nthat the ice was fairly thin and short-lived: insufficient for the$\backslash$naccumulation of the gigantic proglacial lakes that occurred during$\backslash$nearlier glaciations.},
  doi          = {10.1111/j.1751-8369.2008.00061.x},
  file         = {Polyak2008 - The eastern extent of the Barents-Kara ice sheet during the Last Glacial Maximum based on seismic-reflection data from the eastern Kara Sea.pdf:pdf2/Polyak et al/Polyak2008 - The eastern extent of the Barents-Kara ice sheet during the Last Glacial Maximum based on seismic-reflection data from the eastern Kara Sea.pdf:PDF;Polyak2008 - The eastern extent of the Barents-Kara ice sheet during the Last Glacial Maximum based on seismic-reflection data from the eastern Kara Sea.pdf:Polyak2008 - The eastern extent of the Barents-Kara ice sheet during the Last Glacial Maximum based on seismic-reflection data from the eastern Kara Sea.pdf:PDF},
  isbn         = {$08000395\backslash$n17518369},
  journaltitle = {Polar Research},
  keywords     = {Eurasian Arctic margin,Glacigenic sediments,Kara Sea,Last Glacial Maximum,Pleistocene glaciations,Sub-bottom profiling},
}

@Article{Porritt2014,
  author       = {Porritt, Robert W. and Allen, Richard M. and Pollitz, Fred F.},
  title        = {{Seismic imaging east of the Rocky Mountains with USArray}},
  year         = {2014},
  volume       = {402},
  number       = {C},
  pages        = {16--25},
  issn         = {0012-821X},
  abstract     = {USArray has facilitated significant advancement in tomographic models and methodologies. While there persists a fundamental tradeoff between horizontal and vertical resolution due to the components of the wave train analyzed, advances in joint inversions are continuing to refine the tomographic images generated with USArray. The DNA13 model incorporates teleseismic P observations, independent SH and SV observations, and surface-wave phase velocities from both teleseismic earthquakes and ambient noise to constrain the relative wave-speed from the crust down into the lower mantle. We address the validity of our models through forward prediction of observables and compare the predictive power of the DNA13 models to other models. In the shallow portion of DNA13, we image the Archean age Wyoming Province, which exhibits evidence of ocean closure at its northern and southern ends. The Llano Province in Central Texas is of Grenville age and still contains lithospheric evidence of subduction as the province accreted to North America. Comparison of these two provinces highlights the role of fossil slabs as part of the cratonic architecture. Analysis of the deep portion of the models highlights variations within the Farallon plate, including two distinct high wave-speed anomalies in the eastern U.S. and a shallow feature in the center of these two anomalies. We propose this is evidence of an oceanic plateau, which provides the necessary positive buoyancy to promote flat-slab subduction of the Farallon plate.},
  doi          = {10.1016/j.epsl.2013.10.034},
  file         = {Porritt2014 - Seismic imaging east of the Rocky Mountains with USArray.pdf:Porritt2014 - Seismic imaging east of the Rocky Mountains with USArray.pdf:PDF;Porritt2014 - Seismic imaging east of the Rocky Mountains with USArray.pdf:pdf2/Porritt, Allen, Pollitz/Porritt2014 - Seismic imaging east of the Rocky Mountains with USArray.pdf:PDF},
  isbn         = {0012821X},
  journaltitle = {Earth and Planetary Science Letters},
  keywords     = {Deep Farallon slab,Geophysics,Llano Province,Seismology,Wyoming Province},
  publisher    = {Elsevier B.V.},
}

