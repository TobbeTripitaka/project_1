@Article{Baba1992,
  author       = {Baba, V V and Jamal, M},
  title        = {{How much do we really know about glacier surging?}},
  year         = {1992},
  volume       = {21},
  number       = {1},
  pages        = {65--73},
  issn         = {0091-0260},
  abstract     = {A comparison is made of the rate of moonlighting reported in government publications with empirical studies done by researchers. The differences between moonlighters and nonmoonlighters observed in empirical studies on several dependent variables are examined. The average rate of moonlighting in empirical studies is found to be much higher than that reported in government publications in the US and Canada. Only a few differences are found between moonlighters and nonmoonlighters with respect to personal, social, and organizationally-valued outcomes. No significant differences are found with regards to psychosomatic health problems, marital happiness, and job stress. In general, moonlighters experience higher job satisfaction in their primary jobs than do nonmoonlighters. The traditional thinking about moonlighting as "burning the candle from both ends," resulting in high stress at work and at home, is not supported by empirical research},
  doi          = {10.3189/172756403781816185},
  file         = {Baba1992 - How much do we really know about glacier surging_.pdf:Baba1992 - How much do we really know about glacier surging_.pdf:PDF;Baba1992 - How much do we really know about glacier surging_.pdf:pdf2/Baba, Jamal/Baba1992 - How much do we really know about glacier surging_.pdf:PDF},
