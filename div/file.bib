@Article{Chiu2007a,
  author       = {Chiu, Tzu Chien and Fairbanks, Richard G. and Cao, Li and Mortlock, Richard A.},
  title        = {{Analysis of the atmospheric 14{C} record spanning the past 50,000 years derived from high-precision 230{T}h/234U/238U, 231{Pa}/235U and 14{C} dates on fossil corals}},
  year         = {2007},
  volume       = {26},
  number       = {1-2},
  pages        = {18--36},
  issn         = {0277-3791},
  abstract     = {Changes in the geomagnetic field intensity, solar variability, and the internal changes of the carbon cycle are believed to be the three controlling factors of past atmospheric radiocarbon (14C) concentrations (denoted as ??14C). Of these three, it is believed that the field intensity is the dominant factor. We analyze an atmospheric ??14C record spanning the past 50,000 years based on previously-published 230\textsuperscript{Th}/234U/238U and 14C dates of fossil corals from Kiritimati, Barbados, Araki and Santo Islands, and identify the role of the Laschamp geomagnetic field excursion on the long term trend of the ??14C record. There is a general consistency between the coral ??14C record and the ??14C output from carbon cycle models based on the global 14C production estimates. High-precision, high-accuracy 230\textsuperscript{Th}/234U/238U dates and redundant 231Pa/235U dates anchor the timing of this ??14C record. We propose that a significant fraction of the long-term ??14C trend may be due to inaccuracies in the generally accepted 14C decay constant. The uncertainty in estimating the shape of 14C beta spectrum below 20 keV leads to one of the greatest errors in decay constant estimates. Once the 14C half-life is validated via redundant techniques, ??14C records will provide a better opportunity to examine the roles of carbon cycle and 14C production influences. ?? 2006 Elsevier Ltd. All rights reserved.},
  doi          = {10.1016/j.quascirev.2006.06.015},
  file         = {Chiu2007a - Analysis of the atmospheric 14C record spanning the past 50,000 years derived from high-precision 230Th_234U_238U, 231Pa_235U and 14C dates on fossil corals.pdf:Chiu2007a - Analysis of the atmospheric 14C record spanning the past 50,000 years derived from high-precision 230Th_234U_238U, 231Pa_235U and 14C dates on fossil corals.pdf:PDF},
  isbn         = {0277-3791},
  journaltitle = {Quaternary Science Reviews},
  pmid         = {568},
}

@Article{Chiu2005,
  author       = {Chiu, Tzu Chien and Fairbanks, Richard G. and Mortlock, Richard A. and Bloom, Arthur L.},
  title        = {{Extending the radiocarbon calibration beyond 26,000 years before present using fossil corals}},
  year         = {2005},
  volume       = {24},
  number       = {16-17},
  pages        = {1797--1808},
  issn         = {0277-3791},
  abstract     = {Fossil coral is an excellent archive to extend the radiocarbon ( 14C) calibration beyond tree-ring records; however, published coral data older than 26,000 years before present are too disparate for practical calibration. We propose an explanation for these discrepancies: trace amounts of secondary calcite and organic matter in fossil corals shift the 14C ages toward younger ages. In a series of acid-leaching experiments to simulate the standard cleaning procedure prior to 14C analysis, we measure up to a 300{\%} enrichment of diagenetic calcite in corals due to the solubility differences between calcite and aragonite. In model calculations, we show the 14C age offsets produced by acid leaching could be hundreds to thousands of years when typical amounts of calcite are present in samples. We demonstrate the necessity and our ability to detect {\textless}0.2{\%} calcite in aragonite by X-ray diffraction and apply "{\textless}0.2{\%} calcite" as our a priori criterion for our coral samples. The estimated age offsets of samples with {\textless}0.2{\%} calcite fall within the reported analytical uncertainty of 14C dating. In addition, we present 14C results from an extended hydrogen peroxide (H2O2) pretreatment for coral samples to remove organic matter. Our data show that treated samples yield consistent or older 14C dates compared to non-treated samples. These rigorous but necessary screening and cleaning techniques provide precise, reproducible and accurate radiocarbon calibration data. ?? 2005 Elsevier Ltd. All rights reserved.},
  doi          = {10.1016/j.quascirev.2005.04.002},
  file         = {Chiu2005 - Extending the radiocarbon calibration beyond 26,000 years before present using fossil corals.pdf:Chiu2005 - Extending the radiocarbon calibration beyond 26,000 years before present using fossil corals.pdf:PDF},
  isbn         = {0277-3791},
  journaltitle = {Quaternary Science Reviews},
}

@Article{Christiano1999,
