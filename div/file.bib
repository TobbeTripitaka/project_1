@Article{Steig1998,
  author       = {Steig, E. J.},
  title        = {{Synchronous Climate Changes in {Antarctica} and the North Atlantic}},
  year         = {1998},
  volume       = {282},
  number       = {5386},
  pages        = {92--95},
  issn         = {1095-9203},
  abstract     = {Central Greenland ice cores provide evidence of abrupt changes in climate over the past 100,000 years. Many of these changes have also been identified in sedimentary and geochemical signatures in deep-sea sediment cores from the North Atlantic, confirming the link between millennial-scale climate variability and ocean thermohaline circulation. It is shown here that two of the most prominent North Atlantic events-the rapid warming that marks the end of the last glacial period and the Bolling/Allerod-Younger Dryas oscillation-are also recorded in an ice core from Taylor Dome, in the western Ross Sea sector of Antarctica. This result contrasts with evidence from ice cores in other regions of Antarctica, which show an asynchronous response between the Northern and Southern Hemispheres.},
  doi          = {10.1126/science.282.5386.92},
  file         = {Steig1998 - Synchronous Climate Changes in Antarctica and the North Atlantic.pdf:Steig1998 - Synchronous Climate Changes in Antarctica and the North Atlantic.pdf:PDF;Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:pdf2/Steig, Wolfe, Miller/Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:PDF;Steig1998 - Synchronous Climate Changes in Antarctica and the North Atlantic.pdf:pdf2/Steig/Steig1998 - Synchronous Climate Changes in Antarctica and the North Atlantic.pdf:PDF;Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:Steig1998a - Wisconsinan refugia and the glacial history of eastern Baffin Island, Arctic Canada_ Coupled evidence from cosmogenic isotopes and lake sediments.pdf:PDF},
  isbn         = {0036-8075},
