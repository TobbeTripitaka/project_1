@Article{Franke2005,
  author       = {Franke, Dieter and Hinz, K.},
  title        = {{The structural style of sedimentary basins on the shelves of the Laptev Sea and western East Siberian Sea, Siberian Arctic}},
  year         = {2005},
  volume       = {28},
  number       = {3},
  pages        = {269--286},
  issn         = {0141-6421},
  abstract     = {A total of 11,700 km of multichannel seismic reflection data were$\backslash$nacquired during three recent reconnaissance surveys of the wide, shallow$\backslash$nshelves of the Laptev and western East Siberian Seas in the Siberian$\backslash$nArctic Ocean. Three seismic marker horizons were defined and mapped in$\backslash$nboth shelf areas. Their nature and age were predicted on the basis of$\backslash$nregional tectonic and palaeoenvironmental events and corroborated using$\backslash$nonshore geology. To the north of the Laptev Sea, the Gakkel Ridge, an$\backslash$nactive mid-ocean ridge which separates the North American and Eurasian$\backslash$nPlates, abruptly meets the steep slope of the continental shelf which is$\backslash$ncurvilinear in plan view. Extension has affected the Laptev Shelf since$\backslash$nat least the Early Tertiary and has resulted in the formation of three$\backslash$nmajor, generally north-south trending rift basins: the Ust'Lena Rift,$\backslash$nthe Anisin Basin and the New Siberian Basin.$\backslash$nThe Ust'Lena Rift has a minimum east-west width of 300 km at latitude 75$\backslash$ndegrees N and a Cenozoic infill up to 6 s(twt) in thickness. Further to$\backslash$nthe NW of the Laptev Shelf, the downthrown and faulted basement is$\backslash$noverlain by a sub-parallel layered sedimentary succession with a$\backslash$nthickness of 4 s (twt) that thins towards the west. Although this area$\backslash$nwas affected by extension as shown by the presence of numerous faults,$\backslash$nit is not clear whether this depression on the NW Laptev Shelf is$\backslash$ncontinuous with the Ust' Lena Rift.$\backslash$nThe Anisin Basin is located in the northern part of the Laptev Shelf and$\backslash$nhas a Cenozoic sedimentary fill up to 5 s (twt) thick. The deepest part$\backslash$nof the basin trends north-south. To the west is a secondary, NW-SE$\backslash$ntrending depression which is slightly shallower than the main$\backslash$ndepocentre. The overall structure of the basin is a half-graben with the$\backslash$nmajor bounding fault in the east.$\backslash$nThe New Siberian Basin is up to 70 km wide and has a minimum NW-SE$\backslash$nextent of 300 km. The sedimentary fill is up to 4.5 s (twt) thick.$\backslash$nStructurally, the basin is a half-graben with the bounding fault in the$\backslash$neast.$\backslash$nOur data indicate that the rift basins on the Laptev Shelf are not$\backslash$ncontinuous with those on the East Siberian Shelf The latter shelf can$\backslash$nbest be described as an epicontinental platform which has undergone$\backslash$ncontinuous subsidence since the Late Cretaceous. The greatest subsidence$\backslash$noccurred in the NE, as manifested by a major depocentre filled with$\backslash$ninferred (?)Late Cretaceous to Tertiary sediments up to 5 s (twt) thick.},
  doi          = {10.1111/j.1747-5457.2005.tb00083.x},
  file         = {Franke2005 - The structural style of sedimentary basins on the shelves of the Laptev Sea and western East Siberian Sea, Siberian Arctic.pdf:pdf2/Franke, Hinz/Franke2005 - The structural style of sedimentary basins on the shelves of the Laptev Sea and western East Siberian Sea, Siberian Arctic.pdf:PDF;Franke2005 - The structural style of sedimentary basins on the shelves of the Laptev Sea and western East Siberian Sea, Siberian Arctic.pdf:Franke2005 - The structural style of sedimentary basins on the shelves of the Laptev Sea and western East Siberian Sea, Siberian Arctic.pdf:PDF},
  journaltitle = {Journal of Petroleum Geology},
  pmid         = {16453},
}

