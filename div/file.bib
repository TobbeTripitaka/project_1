@Article{Hirano2011,
  author       = {Hirano, Naoto},
  title        = {{Petit-spot volcanism: A new type of volcanic zone discovered near a trench}},
  year         = {2011},
  volume       = {45},
  number       = {2},
  pages        = {157--167},
  issn         = {0016-7002},
  abstract     = {One extremely young volcano (0.05-1 Ma) and other young volcanoes (1.8, 4.2, 6.0, and 8.5 Ma) composed of strongly alkaline magma were recently discovered on the abyssal plain of the Early Cretaceous (135 Ma) Pacific Plate. These volcanoes were dubbed "petit-spots". The petit-spot volcanic province represents more than 8 Myr of activity over a large area (similar to 600 km along the direction of plate motion), but with a relatively small volume of magma production, thus indicating a small supply of heat inconsistent with a hotspot. The low-flux petit-spot volcanoes may be related to the occurrence of a tensional field of lithosphere caused by plate flexure, with the ascending melt derived from a mantle source susceptible to partial melting. Rock samples from the young volcanoes are highly vesicular (up to 60{\%}) despite high hydrostatic pressures at 6000 m water depth, indicating volatile-rich magmas. The depleted heavy rare earth elements and high radiometric isotopic ratios of noble gases indicate the magma was derived from upper mantle. Nevertheless, the low Nd-143/Nd-144, high Sr-87/Sr-86, low Pb-206/Pb-204 and low Pb-207/Pb-204 ratios are similar to enriched or fertile compositions such as oceanic island basalts. These apparently conflicting data are explained by the extremely small degree of partial melting of recycled materials in the degassing mantle of the asthenosphere, probably with carbonate in the source. The petit-spot volcanoes, therefore, provide a unique window into the nature of the oceanic plate and underlying asthenosphere prior to subduction.},
  file         = {Hirano2011 - Petit-spot volcanism_ A new type of volcanic zone discovered near a trench.pdf:Hirano2011 - Petit-spot volcanism_ A new type of volcanic zone discovered near a trench.pdf:PDF;Hirano2011 - Petit-spot volcanism_ A new type of volcanic zone discovered near a trench.pdf:pdf2/Hirano/Hirano2011 - Petit-spot volcanism_ A new type of volcanic zone discovered near a trench.pdf:PDF},
  isbn         = {0016-7002},
  journaltitle = {Geochemical Journal},
  keywords     = {Basalt,Pacific Plate,Petit-spot,Volatiles,Volcano},
}

@Article{Hirano2006,
  author       = {Hirano, Naoto and Takahashi, Eiichi and Yamamoto, Junji and Abe, Natsue and Ingle, Stephanie P. and Kaneoka, Ichiro and Hirata, Takafumi and Kimura, Jun-Ichi and Ishii, Teruaki and Ogawa, Yujiro and Machida, Shiki and Suyehiro, Kiyoshi},
  title        = {{Volcanism in response to plate flexure.}},
  year         = {2006},
  volume       = {313},
  number       = {5792},
  pages        = {1426--1428},
  issn         = {0036-8075},
  abstract     = {Volcanism on Earth is known to occur in three tectonic settings: divergent plate boundaries (such as mid-ocean ridges), convergent plate boundaries (such as island arcs), and hot spots. We report volcanism on the 135 million-year-old Pacific Plate not belonging to any of these categories. Small alkalic volcanoes form from small percent melts and originate in the asthenosphere, as implied by their trace element geochemistry and noble gas isotopic compositions. We propose that these small volcanoes erupt along lithospheric fractures in response to plate flexure during subduction. Minor extents of asthenospheric melting and the volcanoes' tectonic alignment and age progression in the direction opposite to that of plate motion provide evidence for the presence of a small percent melt in the asthenosphere.},
  doi          = {10.1126/science.1128235},
  file         = {Hirano2006 - Volcanism in response to plate flexure..pdf:Hirano2006 - Volcanism in response to plate flexure..pdf:PDF},
  journaltitle = {Science},
  pmid         = {16873612},
}

