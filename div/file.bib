@Article{Wei2007,
  author       = {Wei, Haiquan and Wang, Yu and Jin, Jinyu and Gao, Ling and Yun, Sung Hyo and Jin, Bolu},
  title        = {{Timescale and evolution of the intracontinental Tianchi volcanic shield and ignimbrite-forming eruption, Changbaishan, Northeast China}},
  year         = {2007},
  volume       = {96},
  number       = {1-2},
  pages        = {315--324},
  issn         = {0024-4937},
  abstract     = {Around the Tianchi eruption cone, the Junjianshan Formation which builds the Tianchi basalt shield can be divided into three units. They are, from bottom to top, the Toudao, Baishan and Laofangzixiaoshan units, respectively. Individual basaltic flows of the Tianchi shield vary significantly in volume and in spatial distribution from a few to dozens of kilometers in length, hundreds to thousands of meters in width, and a few to dozens of meters in thickness. K-Ar age data indicate that the Tianchi shield was formed within a very short time period between 2.77 ($\pm$ 0.08) and 0.31 ($\pm$ 0.002) Ma. Our detailed mapping in combination with the K-Ar dating shows that the Toudao unit erupted at $\sim$ 2.77-1.99{~}Ma, the Baishan unit at $\sim$ 1.64-1.11{~}Ma, and the Laofangzixiaoshan at $\sim$ 0.31-0.58{~}Ma. The huge Millennium eruption from the Tianchi Volcano sustained a 35{~}km high column (HT) and lasted a few days, producing the comenditic ignimbrite around the volcano. Wiggle-matching measurements of a sample series of charcoal wood indicate the Millennium eruption might have happened around 1024 AD. Features of eruptions stemming from the same source surrounding the Tianchi Volcano, plus the volcanic evolution from effusive flows to explosive eruption, are interpreted to reflect crustal thickening within this intracontinental tectonic environment. Field observations and lava compositional variations suggest that crystal fractionation and magma mixing or mingling process must have played an important role in the development and evolution of the Tianchi Volcano. {\textcopyright} 2006.},
  doi          = {10.1016/j.lithos.2006.10.004},
  file         = {Wei2007 - Timescale and evolution of the intracontinental Tianchi volcanic shield and ignimbrite-forming eruption, Changbaishan, Northeast China.pdf:pdf2/Wei et al/Wei2007 - Timescale and evolution of the intracontinental Tianchi volcanic shield and ignimbrite-forming eruption, Changbaishan, Northeast China.pdf:PDF;Wei2007 - Timescale and evolution of the intracontinental Tianchi volcanic shield and ignimbrite-forming eruption, Changbaishan, Northeast China.pdf:Wei2007 - Timescale and evolution of the intracontinental Tianchi volcanic shield and ignimbrite-forming eruption, Changbaishan, Northeast China.pdf:PDF},
  journaltitle = {Lithos},
  keywords     = {Ignimbrite,Intracontinental volcanism,Shield-forming eruption,Tianchi Volcano},
}

@Article{Weidick2004,
  author       = {Weidick, Anker and Kelly, Michael and Bennike, Ole},
  title        = {{Late Quaternary development of the southern sector of the {Greenland} Ice Sheet, with particular reference to the Qassimiut lobe}},
  year         = {2004},
  volume       = {33},
  number       = {4},
  pages        = {284--299},
  issn         = {0300-9483},
  abstract     = {The evolution of the southern Greenland Ice Sheet is interpreted from$\backslash$na synthesis of geological data and palaeoclimatic information provided$\backslash$nby the ice-sheet cores. At the Last Glacial Maximum the ice margin$\backslash$nwould have been at the shelf break and the ice sheet was fringed$\backslash$nby shelf ice. Virtually all of the present ice-free land was glaciated.$\backslash$nThe initial ice retreat was controlled by eustatic sea level rise$\backslash$nand was mainly by calving. When temperatures increased, melt ablation$\backslash$nled to further ice-margin retreat and areas at the outer coast and$\backslash$nmountain tops were deglaciated. Retreat was interrupted by a readvance$\backslash$nduring the Neria stade that may correlate with the Younger Dryas$\backslash$ncooling. The abrupt temperature rise at the Younger Dryas-Holocene$\backslash$ntransition led to a fast retreat of the ice margin, and after -9$\backslash$nka BP the ice sheet was smaller than at present. Expansion of the$\backslash$nice cover began in the Late Holocene, with a maximum generally during$\backslash$nthe Little Ice Age. The greatest changes in ice cover occurred in$\backslash$nlowland areas, i.e. in the region of the Qassimiut lobe. The date$\backslash$nof the historical maximum advance shows considerable spatial variability$\backslash$nand varies between AD 1600 and the present. Local anomalous readvances$\backslash$nare seen at possibly 7-8 ka and at c. 2 ka BP. A marked relative$\backslash$nsea level rise is seen in the Late Holocene; this is believed to$\backslash$nreflect a direct glacio-isostatic response to increasing ice load.},
  doi          = {10.1111/j.1502-3885.2004.tb01242.x},
  file         = {Weidick2004 - Late Quaternary development of the southern sector of the Greenland Ice Sheet, with particular reference to the Qassimiut lobe.pdf:pdf2/Weidick, Kelly, Bennike/Weidick2004 - Late Quaternary development of the southern sector of the Greenland Ice Sheet, with particular reference to the Qassimiut lobe.pdf:PDF;Weidick2004 - Late Quaternary development of the southern sector of the Greenland Ice Sheet, with particular reference to the Qassimiut lobe.pdf:Weidick2004 - Late Quaternary development of the southern sector of the Greenland Ice Sheet, with particular reference to the Qassimiut lobe.pdf:PDF},
  journaltitle = {Boreas},
}

@Article{Wenk2011,
  author       = {Wenk, Hans Rudolf and Cottaar, Sanne and Tom??, Carlos N. and McNamara, Allen and Romanowicz, Barbara},
  title        = {{Deformation in the lowermost mantle: From polycrystal plasticity to seismic anisotropy}},
  year         = {2011},
  volume       = {306},
  number       = {1-2},
  pages        = {33--45},
  issn         = {0012-821X},
  abstract     = {In the deep earth, deformation occurs at many scales: large-scale convection produces subduction of slabs and upwelling of plumes in the mantle. At the high temperature/high pressure conditions, strain is accommodated through crystal plasticity, either by diffusion or the movement of dislocations. Slip causes crystal rotations and thus produces a characteristic pattern of crystal preferred orientation and corresponding anisotropy of physical properties at the macroscopic scale. In this study we use polycrystal plasticity, with experimentally derived deformation mechanisms for perovskite, post-perovskite and magnesiowuestite, to predict texture development along streamlines in a 2D geodynamic convection model of the lowermost mantle. Strong preferred orientation develops during subduction and upwelling, while during spreading along the core-mantle boundary the orientation pattern is relatively stable. From preferred crystal orientation and single crystal elastic properties, bulk elastic properties can be calculated and compared with seismic observations. Post-perovskite with predominant (001) slip and magnesiowuestite with {\{}110{\}} and {\{}111{\}} slip produce anisotropy patterns which are consistent with observed anisotropy, i.e. fast S-waves polarized parallel to the core-mantle boundary and anti-correlation between P and S-wave anisotropies. In contrast, perovskite with dominant (001) slip and two post-perovskite models with dominant slip on (010) and (100) produce anisotropy patterns which are inconsistent with seismic observations. ?? 2011 Elsevier B.V.},
  doi          = {10.1016/j.epsl.2011.03.021},
  file         = {Wenk2011 - Deformation in the lowermost mantle_ From polycrystal plasticity to seismic anisotropy.pdf:Wenk2011 - Deformation in the lowermost mantle_ From polycrystal plasticity to seismic anisotropy.pdf:PDF;Wenk2011 - Deformation in the lowermost mantle_ From polycrystal plasticity to seismic anisotropy.pdf:pdf2/Wenk et al/Wenk2011 - Deformation in the lowermost mantle_ From polycrystal plasticity to seismic anisotropy.pdf:PDF},
  isbn         = {0012-821X},
  journaltitle = {Earth and Planetary Science Letters},
  keywords     = {D" anisotropy,Lowermost mantle,Magnesiowuestite,Perovskite,Post-perovskite,Texture development},
  publisher    = {Elsevier B.V.},
}

