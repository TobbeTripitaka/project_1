@Misc{Wysession1998,
  author    = {Wysession, Me and Lay, T and Revenaugh, J and Williams, Q and Garnero, Ej and Jeanloz, R and Kellogg, Lh},
  title     = {{The D$\prime\prime$ discontinuity and its implications}},
  year      = {1998},
  abstract  = {A synthesis is presented of the results from over 40 different studies of a discontinuous increase in seismic velocity, referred to as the {\textquotedblleft}D$\prime\prime$ discontinuity{\textquotedblright}, that occurs about 250 km above the core-mantle boundary. This discontinuity is seen in many regions around the world, both with P and S waves. The D$\prime\prime$ discontinuity has a mean apparent depth of about 250--265 km for both P and S waves. While many more non-observations (where no discontinuity is observable above the seismic noise levels) exist for P waves than for S waves, both P and S non-observations and observations are distributed with little obvious geographical coherence. The O$\prime\prime$ discontinuity is often modeled as a precipitous increase in seismic velocity of about 2.5--3.0{\%}, though most data can accommodate a transition width of up to 50 or 75 km. The D$\prime\prime$ discontinuity is usually modeled with lower than average velocities above the discontinuity, but the velocity gradient beneath the discontinuity has been modeled with negative, positive, or zero velocity gradients. There is a strong indication that the base of D$\prime\prime$ displays a strong decrease in seismic velocities. There is no significant correlation between large scale seismic velocities at the base of the mantle and (1) the occurrence or absence of a D$\prime\prime$ discontinuity, or (2) a variation in the thickness of the observed depth of the discontinuity. Available seismic evidence leaves several possibilities open for D$\prime\prime$ discontinuity origins. Ponding of the remnants of subducted slabs may provide a large enough decrease in temperature to generate a 3{\%} velocity increase. A mineralogical phase change in a major silicate constituent, perhaps caused by thermal or chemical anomalies, might provide a significant velocity increase. In both cases, a mechanism must exist to cause significant topography on the discontinuity. A third possibility is of significant heterogeneity, either as a stable chemical boundary layer or in the form of laminar layers (perhaps from core-mantle reactions or of ancient ocean crust) that are scattered about the base of the mantle.},
  booktitle = {Geodynamic Series},
  doi       = {10.1029/GD028p0273},
  file      = {Wysession1998 - The D″ discontinuity and its implications.pdf:pdf2/Wysession et al/Wysession1998 - The D″ discontinuity and its implications.pdf:PDF;Wysession1998 - The D″ discontinuity and its implications.pdf:Wysession1998 - The D$_prime_prime$ discontinuity and its implications.pdf:PDF},
  issn      = {0277-6669},
  pages     = {273--297},
  volume    = {28},
}

@Article{Xia1999,
  author       = {Xia, Jianghai and Miller, Richard D. and Park, Choon B.},
  title        = {{Estimation of near-surface shear-wave velocity by inversion of {Rayleigh} waves}},
  year         = {1999},
  volume       = {64},
  number       = {NO.3},
  pages        = {691--700},
  issn         = {0016-8033},
  abstract     = {The shear-wave (S-wave) velocity of near-surface materials (soil, rocks, pavement) and its effect on seismic-wave propagation are of fundamental interest in many groundwater, engineering, and environmental studies. Rayleigh-wave phase velocity of a layered-earth model is a function of frequency and four groups of earth properties: P-wave velocity, S-wave velocity, density, and thickness of layers. Analysis of the Jacobian matrix provides a measure of dispersion-curve sensitivity to earth properties. S-wave velocities are the dominant influence on a dispersion curve in a high-frequency range ({\textgreater}5 Hz) followed by layer thickness. An iterative solution technique to the weighted equation proved very effective in the high-frequency range when using the Levenberg-Marquardt and singular-value decomposition techniques. Convergence of the weighted solution is guaranteed through selection of the damping factor using the Levenberg-Marquardt method. Synthetic examples demonstrated calculation efficiency and stability ...},
  doi          = {10.1190/1.1444578},
  file         = {Xia1999 - Estimation of near-surface shear-wave velocity by inversion of Rayleigh waves.pdf:Xia1999 - Estimation of near-surface shear-wave velocity by inversion of Rayleigh waves.pdf:PDF;Xia1999 - Estimation of near-surface shear-wave velocity by inversion of Rayleigh waves.pdf:pdf2/Xia, Miller, Park/Xia1999 - Estimation of near-surface shear-wave velocity by inversion of Rayleigh waves.pdf:PDF},
