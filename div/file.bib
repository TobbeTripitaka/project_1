@Article{Hajdas2007,
  author       = {Hajdas, Irka and Schlumpf, Nadia and Minikus-Stary, Nicole and Hagedorn, Frank and Eckmeier, Eileen and Schoch, Werner and Burga, Conradin and Bonani, Georges and Schmidt, Michael W I and Cherubini, Paolo},
  title        = {{Radiocarbon ages of soil charcoals from the southern Alps, Ticino, Switzerland}},
  year         = {2007},
  volume       = {259},
  number       = {1},
  pages        = {398--402},
  issn         = {0168-583X},
  abstract     = {Radiocarbon dating of macroscopic charcoal is a useful tool for paleoclimatic and paleoecologic reconstructions. Here we present results of 14C dating of charcoals found in charcoal-rich soils of Ticino and the Misox Valley (southern Switzerland) which indicate that the Late Glacial and early Holocene fires coincided with warm phases in the North Atlantic region and low lake levels in the Central Europe. Late Holocene charcoals found in these soils document an earlier than believed presence of sweet chestnut (Castanea sativa Mill.) in southern Switzerland. Sweet chestnut trees play a key role in Mediterranean woodlands, and for longer than two millennia have been used as a food source. Based on palynological evidence it is commonly believed that in southern Switzerland C. sativa was first introduced 2000 years ago by the Romans, who cultivated it for wood and fruit production. Our results indicate that this tree species was present on the southern slopes of the Alps ???1500 years earlier than previously assumed, and therefore was likely introduced independently from cultivation by the Romans. ?? 2007 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.nimb.2007.02.075},
  file         = {Hajdas2007 - Radiocarbon ages of soil charcoals from the southern Alps, Ticino, Switzerland.pdf:Hajdas2007 - Radiocarbon ages of soil charcoals from the southern Alps, Ticino, Switzerland.pdf:PDF;Hajdas2007 - Radiocarbon ages of soil charcoals from the southern Alps, Ticino, Switzerland.pdf:pdf2/Hajdas et al/Hajdas2007 - Radiocarbon ages of soil charcoals from the southern Alps, Ticino, Switzerland.pdf:PDF},
  journaltitle = {Nuclear Instruments and Methods in Physics Research, Section B: Beam Interactions with Materials and Atoms},
  keywords     = {AMS 14C dating,Castanea sativa,Charcoal,Misox Switzerland,Paleo fires},
}

