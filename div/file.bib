@Article{Bonow2006,
  author       = {Bonow, Johan M. and Lidmar-Bergstr{\"{o}}m, Karna and Japsen, Peter},
  title        = {{Palaeosurfaces in central West {Greenland} as reference for identification of tectonic movements and estimation of erosion}},
  year         = {2006},
  volume       = {50},
  number       = {3-4},
  pages        = {161--183},
  issn         = {0921-8181},
  abstract     = {Landform analysis of basement rocks has been undertaken with the aid of digital elevation data, aerial photographs and field observations in central West Greenland (69{$^{\circ}$}15$\prime$N-66{$^{\circ}$}00$\prime$N). Palaeosurfaces have been identified, dated relatively to each other, used to quantify uplift and fault movements and also used to estimate differential erosion. Two types of palaeosurfaces were mapped across the Precambrian basement: a surface at low elevation with distinct hills (hilly relief), and two planation surfaces formed across different types of basement rocks. The hilly relief surface emerges as an inclined surface from Cretaceous cover rocks in Disko Bugt and is interpreted as a stripped late Mesozoic etch surface. This surface is cut off towards the south by a less inclined planation surface, which is younger and thus of Cenozoic age. It is similar to the post-Eocene (Miocene?) planation surfaces identified on Disko and Nuussuaq in other studies. The planation surface splits in two southwards towards high areas around Nordre Isortoq and Sukkertoppen Ice Cap. The upper planation surface forms near-summit areas of tectonic blocks dipping in different directions and with different tilts. The uplift centres define the crests of two mega blocks, separated by the 'Sisimiut Line' which coincides with the Precambrian Ikert{\^{o}}q thrust zone. A partially developed lower planation surface indicates a first uplift of maximum 500{~}m followed by a second uplift of maximum 1000{~}m. We infer that these uplift events occurred during the late Neogene based on correlation with similar surfaces on Nuussuaq and the timing of exhumational events estimated from apatite fission track analyses of samples from a deep borehole on Nuussuaq (reported elsewhere). The difference between a reconstruction of the upper planation surface across the entire area and the present topography was used as an estimate of erosion of basement rock since the formation of the upper planation surface. The erosion is unevenly distributed and varies from almost none on the well-preserved planation surfaces to 800-1300{~}m along valleys, and even more in the fjords. Erosion is less within areas of gneiss in granulite facies, than in areas of gneiss in amphibolite facies. {\textcopyright} 2006 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.gloplacha.2005.12.011},
  isbn         = {0921-8181},
  journaltitle = {Global and Planetary Change},
  keywords     = {Landform analysis,Neogene,West Greenland,differential erosion,etch surface,hilly relief,palaeosurface,planation surface,tectonic movements,uplift},
  pmid         = {17246},
}

