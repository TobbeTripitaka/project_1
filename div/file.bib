@Article{Gjelberg2005,
  author       = {Gjelberg, J and Martinsen, O J and Charnock, M A and Moller, N and Antonsen, P},
  title        = {{The reservoir development of the Late Maastrichtian--Early Paleocene Ormen Lange gas field, M{\o}re Basin, Mid-Norwegian Shelf}},
  year         = {2005},
  pages        = {1165--1185},
  issn         = {2047-9921},
  abstract     = {The giant Ormen Lange Gas Field is situated in the M{\o}re Basin, Norwegian Sea, at water depths around 1000 m. The reservoir interval consists of a lower heterolithic unit mainly of Maastrichtian age (Jorsalfare Formation) and an upper sandstone unit of Danian age (Egga Member, V{\aa}le Formation), separated by a widely distributed mudstone unit ({\textquoteleft}V{\aa}le Tight'), and consists of both high and low density turbidites in association with fine-grained background sediments. The reservoir interval spans the Cretaceous--Tertiary boundary, without any major stratigraphic breaks. The main architectural elements of the reservoir are: channel-dominated deposits, channelized lobe deposits and non-channelized frontal splay or fan fringe deposits. The spatial distribution of these facies elements indicates a dynamic system with changing sediment supply through time. Core and log studies from the wells have been integrated with high-resolution biostratigraphy to develop a dynamic depositional model.},
  doi          = {10.1144/0061165},
  file         = {Gjelberg2005 - The reservoir development of the Late Maastrichtian–Early Paleocene Ormen Lange gas field, M_ore Basin, Mid-Norwegian Shelf.pdf:pdf2/Gjelberg et al/Gjelberg2005 - The reservoir development of the Late Maastrichtian–Early Paleocene Ormen Lange gas field, M_ore Basin, Mid-Norwegian Shelf.pdf:PDF;Gjelberg2005 - The reservoir development of the Late Maastrichtian–Early Paleocene Ormen Lange gas field, M_ore Basin, Mid-Norwegian Shelf.pdf:Gjelberg2005 - The reservoir development of the Late Maastrichtian--Early Paleocene Ormen Lange gas field, M_ore Basin, Mid-Norwegian Shelf.pdf:PDF},
  isbn         = {1-86239-164-5},
  journaltitle = {Petroleum Geology: North-West Europe and Global Perspectives - Proceedings of the 6\textsuperscript{th} Petroleum Geology Conference},
  keywords     = {Deep water plays,Reservoirs},
