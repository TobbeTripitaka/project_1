@Article{Hamilton1975,
  author       = {Hamilton, W and Elliot, DAVID H},
  title        = {{Tectonics of {Antarctica}: a review}},
  year         = {1975},
  volume       = {275},
  number       = {4-6},
  pages        = {45--106},
  issn         = {0040-1951},
  abstract     = {Antarctica consists of large and wholly continental east Antarctica and smaller west Antarctica which would form large and small islands, even after isostatic rebound, if its ice cap were melted. Most of east Antarctica is a Precambrian Shield, in much of which charnockites are characteristic. The high Transantarctic Mountains, along the Ross and Weddell Seas, largely follow a geosyncline of Upper Precambrian sedimentary rocks that were deformed, metamorphosed and intruded by granitic rocks during Late Cambrian or Early Ordovician time. The rocks of the orogen were peneplained, then covered by thin and mostly continental Devonian-Jurassic sediments, which were intruded by Jurassic diabase sheets and overlain by plateau-forming tholeiites. Late Cenozoic doming and block-faulting have raised the present high mountains. Northeastern Victoria Land, the end of the Transantarctic Mountains south of New Zealand, preserves part of a Middle Paleozoic orogen. Clastic strata laid unconformably upon the Lower Paleozoic plutonic complex were metamorphosed at low grade, highly deformed and intruded by Late Devonian or Early Carboniferous granodiorites. The overlying Triassic continental sedimentary rocks have been broadly folded and normal-faulted. Interior west Antarctica is composed of miogeosynclinal clastic and subordinate carbonate rocks which span the Paleozoic Era and which were deformed, metamorphosed at generally low grade, and intruded by granitic rocks during Early Mesozoic time and possibly during other times also. Patterns of orogenic belts, if systematic, cannot yet be defined; but fragmentation and rotation of crustal blocks by oroclinal folding and strike-slip faulting can be suggested. The Ellsworth Mountains, for example, consist of Cambrian-Permian metasedimentary rocks that strike northward toward the noncorrelative and latitudinally striking Mesozoic terrane of the Antarctic Peninsula in one direction and southward toward that of the Lower Paleozoic: terrane of the Transantarctic Mountains in the other; the three regions may be separated by great strike-slip faults. The Antarctic Peninsula in west Antarctica, south of South America, consists of metavolcanic and metasedimentary rocks intruded by Late Cretaceous quartz diorite. The pre-granitic rocks are of Jurassic and Early Cretaceous ages wherever they have been dated by fossils, although some crystalline complexes may be older. The S-shape of the peninsula may represent oroclinal bending within Cenozoic time as part of a motion system in which a narrow continental bridge between South America and Antarctica was deformed and ruptured. Perhaps this bridge lagged behind as the larger continental plates drifted into the Pacific Ocean Basin.},
  arxivid      = {arXiv:1011.1669v3},
  doi          = {10.1016/0040-1951(67)90019-4},
  eprint       = {arXiv:1011.1669v3},
  eprinttype   = {arXiv},
  file         = {Hamilton1975 - Tectonics of Antarctica_ a review.pdf:Hamilton1975 - Tectonics of Antarctica_ a review.pdf:PDF;Hamilton1975 - Tectonics of Antarctica_ a review.pdf:pdf2/Hamilton, Elliot/Hamilton1975 - Tectonics of Antarctica_ a review.pdf:PDF},
  isbn         = {0040-1951},
  journaltitle = {American Journal of Science},
  pmid         = {25246403},
}

@Article{Hammarlund2005,
  author       = {Hammarlund, Dan and Bj{\"{o}}rck, Svante and Buchardt, Bj{\o}rn and Thomsen, Charlotte T.},
  title        = {{Limnic responses to increased effective humidity during the 8200 cal. yr {BP} cooling event in southern Sweden}},
  year         = {2005},
  volume       = {34},
  number       = {4},
  pages        = {471--480},
  issn         = {0921-2728},
  abstract     = {Highly resolved sediment stratigraphic and stable isotope records from a lake in south-central Sweden give evidence of relatively humid summers during the wide-spread cold event at c. 8200 cal. BP. A transient lake-level rise led to increased catchment erosion followed by enhanced phytoplankton production and disturbance of the Chara-dominated algal community. An associated increase in the deposition of organic material resulted in hypolimnetic oxygen deficit and methane formation as inferred from elevated sulphur content of the sediments and strong depletion in C-13 of bulk organic material. In combination with dry and cold winter conditions as revealed by other proxy records, these data suggest the development of a strongly seasonal climate in northwestern Europe in response to altered atmospheric circulation at this stage. Enhanced zonal circulation with frequent cyclonic activity and increased effective humidity probably characterized the summer climate of southern and central Scandinavia, consistent with conditions inferred for continental Europe. In contrast, the winter climate was dominated by blocking high-pressure circulation, giving rise to cold and dry conditions},
  doi          = {10.1007/s10933-005-5614-z},
  file         = {Hammarlund2005 - Limnic responses to increased effective humidity during the 8200 cal. yr BP cooling event in southern Sweden.pdf:pdf2/Hammarlund et al/Hammarlund2005 - Limnic responses to increased effective humidity during the 8200 cal. yr BP cooling event in southern Sweden.pdf:PDF;Hammarlund2005 - Limnic responses to increased effective humidity during the 8200 cal. yr BP cooling event in southern Sweden.pdf:Hammarlund2005 - Limnic responses to increased effective humidity during the 8200 cal. yr BP cooling event in southern Sweden.pdf:PDF},
  isbn         = {0921-2728},
  journaltitle = {Journal of Paleolimnology},
  keywords     = {Carbon isotopes,Holocene,Lake sediments,Oxygen isotopes,Palaeoclimate,Sulphur content},
  pmid         = {123},
}

@Article{Hammer2007,
  author       = {Hammer, O. and Dysthe, D. K. and Jamtveit, B.},
  title        = {{The dynamics of travertine dams}},
  year         = {2007},
  volume       = {256},
  number       = {1-2},
  pages        = {258--263},
  issn         = {0012-821X},
  abstract     = {We present a simple, abstract model for travertine dam formation. The simulation uses a depth-averaged hydrodynamic code, coupled with an empirical precipitation model where surface-normal growth rate is proportional to local flow rate. The dynamics of the resulting formation, coarsening and migration (progradation) of crests is demonstrated and compared with field observations. The local self-enhancement of crests, due to increasing flow rate, in combination with lateral inhibition due to upstream drowning, implies a classical pattern formation system with a characteristic dam size, however this wavelength is not stable but increasing. The model also reproduces crest lobes, step bunching and other processes that contribute to the complex dynamics of travertine systems. ?? 2007 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.epsl.2007.01.033},
  file         = {Hammer2007 - The dynamics of travertine dams.pdf:Hammer2007 - The dynamics of travertine dams.pdf:PDF;Hammer2007 - The dynamics of travertine dams.pdf:pdf2/Hammer, Dysthe, Jamtveit/Hammer2007 - The dynamics of travertine dams.pdf:PDF},
  journaltitle = {Earth and Planetary Science Letters},
  keywords     = {carbonate precipitation,hydrodynamics,travertine terraces},
}

@Article{Hammer2005,
  author       = {Hammer, {\O}yvind and Jamtveit, B. and Benning, L. G. and Dysthe, D. K.},
  title        = {{Evolution of fluid chemistry during travertine formation in the Troll thermal springs, {Svalbard}, Norway}},
  year         = {2005},
  volume       = {5},
  number       = {2},
  pages        = {140--150},
  issn         = {1468-8115},
  abstract     = {Arctic hydrothermal springs at Bockfjorden, Svalbard, have isotope and trace element signatures indicative of derivation from glacial melt waters with minor contribution from seawater. Downstream gradients in water chemistry, isotopic composition and carbonate precipitation rates have been documented for the Troll spring and travertine terrace system. The main controls on the downstream evolution of these parameters are carbon dioxide degassing, calcite precipitation, evaporation and biological activity. The carbonate precipitation rates not only show an approximately parabolic dependence on the calcite supersaturation levels, but depend also on local hydrodynamics. Downstream loss of light isotopes of oxygen and hydrogen can be explained as an effect of evaporation, as estimated using chloride as a conservative marker. Biological activity affects nitrate and bromide concentrations and influences the morphology of calcite precipitates. {\textcopyright} 2005 Blackwell Publishing Ltd.},
  doi          = {10.1111/j.1468-8123.2005.00109.x},
  file         = {Hammer2005 - Evolution of fluid chemistry during travertine formation in the Troll thermal springs, Svalbard, Norway.pdf:pdf2/Hammer et al/Hammer2005 - Evolution of fluid chemistry during travertine formation in the Troll thermal springs, Svalbard, Norway.pdf:PDF;Hammer2005 - Evolution of fluid chemistry during travertine formation in the Troll thermal springs, Svalbard, Norway.pdf:Hammer2005 - Evolution of fluid chemistry during travertine formation in the Troll thermal springs, Svalbard, Norway.pdf:PDF},
  journaltitle = {Geofluids},
  keywords     = {Arctic hot springs,Carbonate precipitation,Fluid evolution,Travertine},
}

@Article{Hammond2011,
  author       = {Hammond, William C. and Blewitt, Geoffrey and Kreemer, Corn??},
  title        = {{Block modeling of crustal deformation of the northern Walker Lane and Basin and Range from {GPS} velocities}},
  year         = {2011},
  volume       = {116},
  number       = {4},
  pages        = {1--28},
  issn         = {2169-9356},
  abstract     = {We infer rates of crustal deformation in the northern Walker Lane$\backslash$n(NWL) and western Basin and Range using data from the Mobile Array$\backslash$nof GPS for Nevada transtension, and other continuous GPS networks$\backslash$nincluding the EarthScope Plate Boundary Observatory. We present 224$\backslash$nnew GPS velocities, correct them for the effects of viscoelastic$\backslash$npostseismic relaxation, and use them to constrain a block model to$\backslash$nestimate fault slip rates. The data segregate the NWL into domains$\backslash$nbased on differences in deformation rate, pattern, and style. Deformation$\backslash$nis transtensional, with highest rates near the western and eastern$\backslash$nedges of the NWL. Some basins, e.g., Tahoe, experience shear deformation$\backslash$nand extension. Normal slip is distributed throughout the NWL and$\backslash$nBasin and Range, where 11 subparallel range-bounding normal fault$\backslash$nsystems have an average horizontal extension rate of 0.1 mm/yr. Comparison$\backslash$nbetween geologic and geodetic slip rates indicates that out of 12$\backslash$npublished geologic rates, 10 agree with geodetic rates to within$\backslash$nuncertainties. This suggests that smaller crustal blocks move steadily,$\backslash$nsimilar to larger lithospheric plates, and that geodetic measurements$\backslash$nof slip rates are reliable in zones of complex crustal deformation.$\backslash$nFor the two slip rates that disagree, geologic rates are greater.$\backslash$nThe vertical axis rotation rate of the Carson domain is {\&}{\#}8722;1.3$\backslash$n�0.1�My clockwise, lower than the 3�to 6�My obtained in paleomagnetic$\backslash$nmeasurements. This suggests that vertical axis rotation rates may$\backslash$nhave decreased over the last 9{\&}{\#}8211;13 My as the role of faulting$\backslash$nhas increased at the expense of rigid rotations.},
  doi          = {10.1029/2010JB007817},
  file         = {Hammond2011 - Block modeling of crustal deformation of the northern Walker Lane and Basin and Range from GPS velocities.pdf:Hammond2011 - Block modeling of crustal deformation of the northern Walker Lane and Basin and Range from GPS velocities.pdf:PDF;Hammond2011 - Block modeling of crustal deformation of the northern Walker Lane and Basin and Range from GPS velocities.pdf:pdf2/Hammond, Blewitt, Kreemer/Hammond2011 - Block modeling of crustal deformation of the northern Walker Lane and Basin and Range from GPS velocities.pdf:PDF},
  journaltitle = {Journal of Geophysical Research: Solid Earth},
  pmid         = {21451934},
}

