@Article{Riis2008,
  author       = {Riis, Fridtjof and Lundschien, Bj{\o}rn A. and H{\o}y, Tore and M{\o}rk, Atle and M{\o}rk, M. B E},
  title        = {{Evolution of the Triassic shelf in the northern Barents Sea region}},
  year         = {2008},
