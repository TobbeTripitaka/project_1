@Article{Agnew2011,
  author       = {Agnew, John},
  title        = {{Space and place}},
  year         = {2011},
  volume       = {2011},
  pages        = {316--330},
  abstract     = {The question of space and place in geographical knowledge is ultimately not just about whether the question of {\textquotedblleft}where{\textquotedblright} matters in the way that {\textquotedblleft}when{\textquotedblright} does in explaining {\textquotedblleft}how{\textquotedblright} and even {\textquotedblleft}why{\textquotedblright} something happens. It is also about how it matters. Given that both space and place are about the {\textquotedblleft}where{\textquotedblright} of things and their relative invocation has usually signaled different understandings of what {\textquotedblleft}where{\textquotedblright} means, it is best to examine them together rather than separately. That is the purpose of this chapter.},
  doi          = {10.4135/9781446201091.n24},
  file         = {Agnew2011 - Space and place.pdf:Agnew2011 - Space and place.pdf:PDF;Agnew2011 - Space and place.pdf:pdf2/Agnew/Agnew2011 - Space and place.pdf:PDF},
  isbn         = {9781412910811},
  journaltitle = {Handbook of geographical knowledge},
}

@Article{Aitken2014,
  author        = {Aitken, A. R A and Betts, P. G. and Young, D. A. and Blankenship, D. D. and Roberts, J. L. and Siegert, M. J.},
  title         = {{The Australo-Antarctic Columbia to Gondwana transition}},
  year          = {2014},
  volume        = {29},
  number        = {1},
  pages         = {136--152},
  issn          = {1342-937X},
  __markedentry = {[tobiasstal:1]},
  abstract      = {From the Mesoproterozoic to Cambrian, Australo-Antarctica was characterised by tectonic reconfiguration as part of the supercontinents Columbia, Rodinia and Gondwana. New tectonic knowledge of the Wilkes Land region of Antarctica allows Australo-Antarctic tectonic linkages to be resolved through reconstruction into ca. 160. Ma Gondwana. We also resolve 330.. 30. km of sinistral strike-slip offset on the {\textgreater} 3000. km long Mundrabilla-Frost Shear Zone and 260. 20. km of dextral offset on the {\textgreater} 1000. km long Aurora Fault to reconstruct the ca. 1150. Ma geometry of Australo-Antarctica. Using this revised geometry, we derive the first model of the Columbia to Gondwana reconfiguration process that is geometrically constrained to {\~{}}. 100. km scale. In this model, early Mesoproterozoic tectonics is driven by two opposing subduction systems. A dominantly west-dipping subduction zone existed at the eastern margin of Australo-Antarctica until ca. 1.55-1.50. Ga. A predominantly east-dipping subduction zone operated at the western margin of the Mawson Craton from ca. 1.70. Ga to ca. 1.42. Ga. The latter caused gradual westwards motion and clockwise rotation of the Mawson Craton relative to the West and North Australian Craton and the accretion of a series of continental ribbons now preserved in the Musgrave Province and its southern extensions. A mid-Mesoproterozoic switch to predominantly west-dipping subduction beneath the West Australian Craton brought about the final closure of the Mawson Craton with the North and West Australian Craton along the Rodona-Totten Shear Zone. Convergence was achieved prior to 1.31. Ga, but final collision may not have occurred until ca. 1.29. Ga. Post-1.29. Ga intraplate activity involved prolonged high-temperature orogenesis from 1.22 to 1.12. Ga, and significant movement on the Mundrabilla-Frost Shear Zone between 1.13 and 1.09. Ga, perhaps in response to the assembly of Rodinia at ca. 1.1. Ga. The Australo-Antarctic Craton was amalgamated with Indo-Antarctica along the Indo-Australo-Antarctic Suture (IAAS) and Kuunga Orogeny, probably in the latest Neoproterozoic to early Cambrian.},
  doi           = {10.1016/j.gr.2014.10.019},
  file          = {Aitken2014 - The Australo-Antarctic Columbia to Gondwana transition.pdf:Aitken2014 - The Australo-Antarctic Columbia to Gondwana transition.pdf:PDF},
  isbn          = {1342-937X},
  journaltitle  = {Gondwana Research},
  keywords      = {Antarctica,Australia,Columbia,Gondwana,Rodinia},
  publisher     = {International Association for Gondwana Research},
}

