@Article{Shakesby2004,
  author       = {Shakesby, Richard a. and Matthews, John a. and Winkler, Stefan},
  title        = {{Glacier variations in Breheimen, southern {Norway}: relative-age dating of Holocene moraine complexes at six high-altitude glaciers}},
  year         = {2004},
  volume       = {14},
  pages        = {899--910},
  issn         = {0959-6836},
  abstract     = {Lichenometric evidence and Schmidt hammer R-values are used to date Holocene moraine sequences in front of six high-altitude ({\textgreater}1500 m) glaciers in Breheimen, central southern Norway. At three glacier forelands with southerly aspects (Hogsetbreen, Vestre Hoybre and Ostre Hoybre), relatively small (less than or equal to4 m high) discrete boulder moraine ridges are shown to date from the 'Little Ice Age'. The remaining three glaciers ( Ostre Tundradalskyrkjabre, Nordre Tundradalskyrkjabre and Hestdalshogdibreen), with northerly or easterly aspects and large, multiridged, rampart-like moraine complexes (15-30 m), thought to be ice-cored and of the 'push-deformation' type, lie above the local altitudinal limit of permafrost. Negatively skewed R-value distributions from these moraine complexes provide critical evidence for pre-'Little Ice Age' glacier expansion as they indicate the survival of long-weathered boulders on moraine surfaces where late-lying snow may reduce or prevent lichen growth. Formation of the moraine complexes appears to have begun in Preboreal times, most likely during the Erdalen Event, but much of their present character was determined by repeated Neoglacial glacier expansion, moraine deformation and ridge formation in the late Holocene, culminating in the 'Little Ice Age' readvance.},
  doi          = {10.1191/0959683603hl766rp},
  file         = {Shakesby2004 - Glacier variations in Breheimen, southern Norway_ relative-age dating of Holocene moraine complexes at six high-altitude glaciers.pdf:Shakesby2004 - Glacier variations in Breheimen, southern Norway_ relative-age dating of Holocene moraine complexes at six high-altitude glaciers.pdf:PDF;Shakesby2004 - Glacier variations in Breheimen, southern Norway_ relative-age dating of Holocene moraine complexes at six high-altitude glaciers.pdf:pdf2/Shakesby, Matthews, Winkler/Shakesby2004 - Glacier variations in Breheimen, southern Norway_ relative-age dating of Holocene moraine complexes at six high-altitude glaciers.pdf:PDF},
  isbn         = {0959-6836},
  journaltitle = {The Holocene},
  keywords     = {boulder weathering,erdalen event,glacier variations,hammer,holocene,lichenometry,little ice age,neoglacia-,norway,preboreal,push-deformation moraines,rhizocarpon geographicum,schmidt,tion},
}

