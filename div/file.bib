@Article{Otto-Bliesner2007,
  author       = {Otto-Bliesner, B. L. and Hewitt, C. D. and Marchitto, T. M. and Brady, E. and Abe-Ouchi, A. and Crucifix, M. and Murakami, S. and Weber, S. L.},
  title        = {{Last Glacial Maximum ocean thermohaline circulation: PMIP2~{m}odel intercomparisons and data constraints}},
  year         = {2007},
  volume       = {34},
  number       = {12},
  pages        = {1--6},
  issn         = {0094-8276},
  abstract     = {The ocean thermohaline circulation is important for transports of heat and the carbon cycle. We present results from PMIP2 coupled atmosphere-ocean simulations with four climate models that are also being used for future assessments. These models give very different glacial thermohaline circulations even with comparable circulations for present. An integrated approach using results from these simulations for Last Glacial Maximum (LGM) with proxies of the state of the glacial surface and deep Atlantic supports the interpretation from nutrient tracers that the boundary between North Atlantic Deep Water and Antarctic Bottom Water was much shallower during this period. There is less constraint from this integrated reconstruction regarding the strength of the LGM North Atlantic overturning circulation, although together they suggest that it was neither appreciably stronger nor weaker than modern. Two model simulations identify a role for sea ice in both hemispheres in driving the ocean response to glacial forcing. Citation: Otto-Bliesner, B. L., C. D. Hewitt, T. M. Marchitto, E. Brady, A. Abe-Ouchi, M. Crucifix, S. Murakami, and S. L. Weber (2007), Last Glacial Maximum ocean thermohaline circulation: PMIP2 model intercomparisons and data constraints, Geophys. Res. Lett., 34, L12706, doi:10.1029/ 2007GL029475.},
  doi          = {10.1029/2007GL029475},
  file         = {Otto-Bliesner2007 - Last Glacial Maximum ocean thermohaline circulation_ PMIP2~model intercomparisons and data constraints.pdf:Otto-Bliesner2007 - Last Glacial Maximum ocean thermohaline circulation_ PMIP2~model intercomparisons and data constraints.pdf:PDF;Otto-Bliesner2007 - Last Glacial Maximum ocean thermohaline circulation_ PMIP2~model intercomparisons and data constraints.pdf:pdf2/Otto-Bliesner et al/Otto-Bliesner2007 - Last Glacial Maximum ocean thermohaline circulation_ PMIP2~model intercomparisons and data constraints.pdf:PDF},
  isbn         = {0094-8276},
  journaltitle = {Geophysical Research Letters},
}

@Article{Overpeck1997,
  author       = {Overpeck, J. and Hughen, K. and Hardy, D. and Bradley, R. and Case, R. and Douglas, M. and Finney, B. and Gajewski, K. and Jacoby, G. and Jennings, A. and Lamoureux, S. and Lasca, A. and MacDonald, G. and Moore, J. and Retelle, M. and Smith, S. and Wolfe, A. and Zielinski, G.},
