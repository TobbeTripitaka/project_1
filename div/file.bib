@Misc{Brigham-Grette1995,
  author    = {Brigham-Grette, Julie and Hopkins, David M},
  title     = {{Emergent marine record and paleoclimate of the last interglaciation along the northwest Alaskan coast}},
  year      = {1995},
  abstract  = {The last interglacial high sea-level stand, the Pelukian transgression of isotope substage 5e, is recorded along the western and northern coasts of Alaska by discontinuous but clearly traceable marine terraces and coastal landforms up to about 10 m altitude. The stratigraphy indicates that sea level reached this altitude only once during the last interglacial cycle. From the type area at Nome, to St. Lawrence Island in the Bering Sea, to the eastern limit of the Beaufort Sea, Pelukian deposits contain extralimital faunas indicating that coastal waters were warmer than present. Amino acid ratios in molluscs from these deposits decrease to the north toward Barrow, consistent with the modern regional temperature gradient. Fossil assemblages at Nome and St. Lawrence Island suggest that the winter sea-ice limit was north of Bering Strait, at least 800 km north of its present position, and the Bering Sea was perennially ice-free. Microfauna in Pelukian sediments recovered from boreholes indicate that Atlantic water may have been present on the shallow Beaufort Shelf, suggesting that the Arctic Ocean was not stratified and the Arctic sea-ice cover was not perennial for some period. In coastal regions of western Alaska, spruce woodlands extended westward beyond their modern range and in northern Alaska, on the Arctic Coastal Plain, spruce groves may have entered the upper Colville River basin. The Flaxman Member of the Gubik Formation on the Alaskan Arctic Coastal Plain was deposited during marine isotope substage 5a and records the breakup of an intra-stage 5 ice sheet over northwestern Keewatin.},
  booktitle = {Quaternary Research},
  doi       = {10.1006/qres.1995.1017},
  file      = {Brigham-Grette1995 - Emergent marine record and paleoclimate of the last interglaciation along the northwest Alaskan coast.pdf:pdf2/Brigham-Grette, Hopkins/Brigham-Grette1995 - Emergent marine record and paleoclimate of the last interglaciation along the northwest Alaskan coast.pdf:PDF;Brigham-Grette1995 - Emergent marine record and paleoclimate of the last interglaciation along the northwest Alaskan coast.pdf:Brigham-Grette1995 - Emergent marine record and paleoclimate of the last interglaciation along the northwest Alaskan coast.pdf:PDF},
  issn      = {0033-5894},
  pages     = {159--173},
  volume    = {43},
}

@Article{Brigham-Grette2001a,
  author       = {Brigham-Grette, Julie and Hopkins, David M. and Ivanov, Victor F. and Basilyan, Alexander E. and Benson, Sonja L. and Heiser, Patricia A. and Pushkar, Vladimir S.},
  title        = {{Last interglacial (isotope stage 5) glacial and sea-level history of coastal Chukotka Peninsula and St. Lawrence Island, Western Beringia}},
  year         = {2001},
  volume       = {20},
  number       = {1-3},
  pages        = {419--436},
  issn         = {0277-3791},
  abstract     = {Study of glacial and marine sequences along the outer coast of Chukotka Peninsula, Bering Strait, is the basis for major revisions of the regional stratigraphy. The Val'katlen Suite at its type section at the mouth of the Enmelen River records the youngest high sea stand of the last interglaciation on Chukotka Peninsula. Marine deposits and glacial diamicton stratigraphically below the type section record, respectfully, the peak of the last interglacial (marine oxygen-isotope substage 5e) and rapid glacierization of the coastal mountains probably during substage 5d or 5b. Correlative deposits recording a similar sequence of intra-stage 5 events, but once thought to be of early and middle Pleistocene age, include the Upper and Lower Pinakul' Suite at Cape Pinakul' grid marine deposits near the Nunyamo River on the Gulf of Anadyr, based upon amino-acid analyses, biostratigraphy, and supporting geochronology. These deposits enclose warmer-than-present faunas and floras comparible to last interglacial Pelukian marine deposits found along the coast of Alaska. The last major advance of glacial ice from Chukotka Peninsula across Anadyr Strait and onto St. Lawrence Island was likely initiated during the part of stage 5 based on our reinterpretaion of the stratigraphy at Cape Pinakul', near the Nunyamo River, and on northwestern St. Lawrence Island. Chukotkan ice probably reached St. Lawrence Island sometime during oxygen-isotope stage 4. Ice extend across Chukotka during the LGM was very imited. {\textcopyright} 2000 Elsevier Science Ltd.},
  doi          = {10.1016/S0277-3791(00)00107-4},
  file         = {Brigham-Grette2001a - Last interglacial (isotope stage 5) glacial and sea-level history of coastal Chukotka Peninsula and St. Lawrence Island, Western Beringia.pdf:pdf2/Brigham-grette et al/Brigham-Grette2001a - Last interglacial (isotope stage 5) glacial and sea-level history of coastal Chukotka Peninsula and St. Lawrence Island, Western Beringia.pdf:PDF;Brigham-Grette2001a - Last interglacial (isotope stage 5) glacial and sea-level history of coastal Chukotka Peninsula and St. Lawrence Island, Western Beringia.pdf:Brigham-Grette2001a - Last interglacial (isotope stage 5) glacial and sea-level history of coastal Chukotka Peninsula and St. Lawrence Island, Western Beringia.pdf:PDF},
  isbn         = {0277-3791},
  journaltitle = {Quaternary Science Reviews},
}

@Article{Briner2007,
  author       = {Briner, J. P. and Axford, Y. and Forman, S. L. and Miller, G. H. and Wolfe, A. P.},
  title        = {{Multiple generations of interglacial lake sediment preserved beneath the Laurentide Ice Sheet}},
  year         = {2007},
  volume       = {35},
  number       = {10},
  pages        = {887--890},
  issn         = {0091-7613},
  abstract     = {It is generally assumed that regions glaciated by continental ice sheets offer little promise for long paleoenvironmental records due to erosional processes associated with glaciation. We show that beneath portions of the northeastern Laurentide Ice Sheet, characterized by cold-based glaciation, sediment sequences representing multiple interglaciations have been preserved within extant lake basins. Radiocarbon and optically stimulated luminescence dating confirm the antiquity of the sediments, thereby extending the terrestrial paleoenvironmental record of the Canadian Arctic by hundreds of thousands of years. The lake sediment record presented here corroborates numerous recent cosmogenic exposure dating studies indicating complex patterns of erosion beneath polar ice sheets. It also demonstrates that the presence of intact interglacial sediments does not demand unglaciated refugia. Similarly ancient sediments may be preserved in many regions formerly covered by Pleistocene ice sheets.},
  doi          = {10.1130/G23812A.1},
  file         = {Briner2007 - Multiple generations of interglacial lake sediment preserved beneath the Laurentide Ice Sheet.pdf:Briner2007 - Multiple generations of interglacial lake sediment preserved beneath the Laurentide Ice Sheet.pdf:PDF;Briner2007 - Multiple generations of interglacial lake sediment preserved beneath the Laurentide Ice Sheet.pdf:pdf2/Briner et al/Briner2007 - Multiple generations of interglacial lake sediment preserved beneath the Laurentide Ice Sheet.pdf:PDF},
  isbn         = {0091-7613},
  journaltitle = {Geology},
  keywords     = {Arctic,Cold-based ice,Glacier erosion,Interglaciation,Lake sediments,Laurentide Ice Sheet,Paleoclimate},
}

@Article{Briner2009,
  author       = {Briner, Jason P. and Davis, P. Thompson and Miller, Gifford H.},
  title        = {{Latest Pleistocene and Holocene glaciation of Baffin Island, Arctic {Canada}: key patterns and chronologies}},
  year         = {2009},
  volume       = {28},
  number       = {21-22},
  pages        = {2075--2087},
  issn         = {0277-3791},
  abstract     = {Melting glaciers and ice caps on Baffin Island contribute roughly half of the sea-level rise from all ice in Arctic Canada, although they comprise only one-fourth of the total ice in the region. The uncertain future response of arctic glaciers and ice caps to climate change motivates the use of paleodata to evaluate the sensitivity of glaciers to past warm intervals and to constrain mechanisms that drive glacier change. We review the key patterns and chronologies of latest Pleistocene and Holocene glaciation on Baffin Island. The deglaciation by the Laurentide Ice Sheet occurred generally slowly and steadily throughout the Holocene to its present margin (Barnes Ice Cap) except for two periods of rapid retreat: An early interval $\sim$12 to 10 ka when outlet glaciers retreated rapidly through deep fiords and sounds, and a later interval $\sim$7 ka when ice over Foxe Basin collapsed. In coastal settings, alpine glaciers were smaller during the Younger Dryas period than during the Little Ice Age. At least some alpine glaciers apparently survived the early Holocene thermal maximum, which was several degrees warmer than today, although data on glacier extent during the early Holocene is extremely sparse. Following the early Holocene thermal maximum, glaciers advanced during Neoglaciation, beginning in some places as early as $\sim$6 ka, although most sites do not record near-Little Ice Age positions until $\sim$3.5 to 2.5 ka. Alpine glaciers reached their largest Holocene extents during the Little Ice Age, when temperatures were $\sim$1-1.5 {$^{\circ}$}C cooler than during the late 20\textsuperscript{th} century. Synchronous advances across Baffin Island throughout Neoglaciation indicate sub-Milankovitch controls on glaciation that could involve major volcanic eruptions and solar variability. Future work should further elucidate the state of glaciers and ice caps during the early Holocene thermal maximum and glacier response to climate forcing mechanisms. {\textcopyright} 2008 Elsevier Ltd. All rights reserved.},
  doi          = {10.1016/j.quascirev.2008.09.017},
  file         = {Briner2009 - Latest Pleistocene and Holocene glaciation of Baffin Island, Arctic Canada_ key patterns and chronologies.pdf:Briner2009 - Latest Pleistocene and Holocene glaciation of Baffin Island, Arctic Canada_ key patterns and chronologies.pdf:PDF;Briner2009 - Latest Pleistocene and Holocene glaciation of Baffin Island, Arctic Canada_ key patterns and chronologies.pdf:pdf2/Briner, Davis, Miller/Briner2009 - Latest Pleistocene and Holocene glaciation of Baffin Island, Arctic Canada_ key patterns and chronologies.pdf:PDF},
  isbn         = {0277-3791},
  journaltitle = {Quaternary Science Reviews},
  publisher    = {Elsevier Ltd},
}

@Article{Briner2005a,
  author       = {Briner, Jason P. and Kaufman, Darrell S. and Manley, William F. and Finkel, Robert C. and Caffee, Marc W.},
  title        = {{Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska}},
  year         = {2005},
  volume       = {117},
  number       = {7-8},
  pages        = {1108--1120},
  issn         = {0016-7606},
  abstract     = {Seventy-three new Be/Al ages from 57 moraine boulders and 2 tors, together with 43 previously published cosmogenic exposure ages from 41 moraine boulders, allow us to critique the use of cosmogenic exposure (CE) dating of moraine boulders in Alaska. Boulder exhumation during moraine degradation likely gives rise to the largest uncertainty in constraining the timing of initial moraine stabilization following ice retreat. Isotopic inheritance appears to be most important for moraines deposited close to their cirque headwalls. Boulder-surface (bedrock) erosion rate can be roughly constrained and leads to a range in moraine stabilization ages. Snow-cover history is difficult to constrain, but its effect is thought to be minor for the tall boulders sampled. Despite these complications, the CE ages provide important new information regarding the timing of the last and penultimate glaciations in Alaska. Three penultimate moraines yielded CE ages that overlap with marine isotope stage (MIS) 4/early MIS 3 (4565 ka) rather than MIS 6 (ca. 140 ka). Based on a combination of our new CE chronologies and existing C ages from six study areas, glaciers retreated from their local late Wisconsin maxima: ca. 2427 ka, Kokrines Hills (west-interior Alaska); ca. 2426 ka, northeastern Brooks Range (NE Alaska); ca. 2123 ka, Yukon Tanana Upland (east-interior Alaska); ca. 22 ka, Ahklun Mountains (SW Alaska); ca. 20 ka, western Alaska Range (central Alaska); ca. 1618 ka, Chuilnuk Mountains (SW Alaska). Overall, glacier retreat was concurrent with the peak of the last global glacial maximum, probably in response to limited moisture availability.},
  doi          = {10.1130/B25649.1},
  file         = {Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:pdf2/Briner et al/Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:PDF;Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:PDF},
  isbn         = {00167606},
  journaltitle = {Bulletin of the Geological Society of America},
  keywords     = {Alaska,Cosmogenic exposure dating,Last glacial maximum,Moraine,Penultimate glaciation},
}

@Article{Briner2002,
  author       = {Briner, Jason P. and Kaufman, Darrell S. and Werner, Al and Caffee, Marc and Levy, Laura and Manley, William F. and Kaplan, Michael R. and Finkel, Robert C.},
  title        = {{Glacier readvance during the late glacial (Younger Dryas?) in the Ahklun Mountains southwestern Alaska}},
  year         = {2002},
  volume       = {30},
  number       = {8},
  pages        = {679--682},
  issn         = {0091-7613},
  abstract     = {An expansion of alpine glaciers during the latest Pleistocene produced an extraordinarily well defined end moraine system in the Ahklun Mountains, southwestern Alaska. These moraines, deposited during the Mount Waskey advance, are several kilometers beyond modern glacier termini, and [{\~{}}]80 km upvalley of the late Wisconsin Ahklun Mountains ice cap terminal moraine. Eleven cosmogenic 10Be and 26Al exposure ages on moraine boulders, combined with radiocarbon ages from a lake core upvalley of a moraine deposited during the Mount Waskey advance, suggest that the advance culminated between 12.4 and 11.0 ka, sometime during, or shortly following, the Younger Dryas event (ca. 12.9-11.6 ka). We believe that the Mount Waskey advance was a consequence of cooling during the Younger Dryas. These data further strengthen emerging evidence for Younger Dryas-age cooling of the North Pacific region.},
  doi          = {10.1130/0091-7613(2002)030<0679:GRDTLG>2.0.CO;2},
  file         = {Briner2002 - Glacier readvance during the late glacial (Younger Dryas_) in the Ahklun Mountains southwestern Alaska.pdf:Briner2002 - Glacier readvance during the late glacial (Younger Dryas_) in the Ahklun Mountains southwestern Alaska.pdf:PDF;Briner2002 - Glacier readvance during the late glacial (Younger Dryas_) in the Ahklun Mountains southwestern Alaska.pdf:pdf2/Briner et al/Briner2002 - Glacier readvance during the late glacial (Younger Dryas_) in the Ahklun Mountains southwestern Alaska.pdf:PDF},
  isbn         = {0091-7613},
  journaltitle = {Geology},
  keywords     = {Alaska,Cosmogenic exposure dating,Glaciers,Late glacial,Younger Dryas},
  pmid         = {2444091},
}

@Article{Briner2003,
  author       = {Briner, J. P. and Miller, G. H. and Davis, P. T. and Bierman, P. R. and Caffee, M.},
  title        = {{Last glacial maximum ice sheet dynamics in Arctic {Canada} inferred from young erratics perched on ancient tors}},
  year         = {2003},
  volume       = {22},
  number       = {5-7},
  pages        = {437--444},
  issn         = {0277-3791},
  abstract     = {A long-standing debate regarding the reconstruction of former ice sheets revolves around the use of relative weathering of landscapes, i.e., the assumption that highly weathered landscapes have not been recently glaciated. New cosmogenic isotope measurements from upland bedrock surfaces and erratics along the northeastern margin of the Laurentide Ice Sheet (LIS) shed light on this debate, 10Be and 26Al concentrations from three perched erratics, yielding cosmogenic exposure ages of 17-11 ka, are much lower than those measured in two unmodified, highly weathered tors upon which they lie, which yield cosmogenic exposure ages of {\textgreater} 60 ka. These findings suggest that non-erosive ice covered weathered upland surfaces along the northeastern margin of the LIS during the last glacial maximum. These data challenge the use of relative weathering to define the margins of Pleistocene ice sheets. The juxtaposition of non-erosive ice over upland plateaus and erosive ice in adjacent fiords requires strong gradients in basal thermal regimes, suggestive of an ice-stream mode of glaciation. ?? 2003 Elsevier Science Ltd. All rights reserved.},
  doi          = {10.1016/S0277-3791(03)00003-9},
  file         = {Briner2003 - Last glacial maximum ice sheet dynamics in Arctic Canada inferred from young erratics perched on ancient tors.pdf:Briner2003 - Last glacial maximum ice sheet dynamics in Arctic Canada inferred from young erratics perched on ancient tors.pdf:PDF;Briner2003 - Last glacial maximum ice sheet dynamics in Arctic Canada inferred from young erratics perched on ancient tors.pdf:pdf2/Briner et al/Briner2003 - Last glacial maximum ice sheet dynamics in Arctic Canada inferred from young erratics perched on ancient tors.pdf:PDF},
  isbn         = {0277-3791},
  journaltitle = {Quaternary Science Reviews},
}

@Article{Briner2005,
  author       = {Briner, Jason P and Miller, Gifford H and Davis, P Thompson and Finkel, Robert C},
  title        = {{Cosmogenic exposure dating in arctic glacial landscapes: implications for the glacial history of northeastern Baffin Island, Arctic Canada}},
  year         = {2005},
  volume       = {42},
  number       = {1},
  pages        = {67--84},
  issn         = {0008-4077},
  abstract     = {Cosmogenic exposure dating and detailed glacial-terrain mapping from$\backslash$nthe Clyde Foreland, Baffin Island, Arctic Canada, reveal new information$\backslash$nabout the extent and dynamics of the northeastern sector of the Laurentide$\backslash$nIce Sheet (LIS) during the last glacial maximum (LGM). The Clyde$\backslash$nForeland is composed of two distinct landscape zones: (1) glacially$\backslash$nscoured terrain proximal to the major sources of Laurentide ice that$\backslash$nflowed onto the foreland, and (2) ice distal unscoured sectors of$\backslash$nthe foreland. Both zones are draped with erratics and dissected by$\backslash$nmeltwater channels, indicating past ice cover. We interpret the two$\backslash$nlandscape classes in terms of ice sheet erosive ability linked with$\backslash$nbasal thermal regime: glacially scoured terrain was occupied by erosive$\backslash$nwarm-based ice, and unscoured terrain was last occupied by non-erosive$\backslash$ncold-based ice. Cosmogenic exposure ages from {\textgreater} 100 erratics from$\backslash$nthe two landscape types have different age distributions. Cosmogenic$\backslash$nexposure ages from the glacially scoured areas suggest ice cover$\backslash$nduring the LGM, followed by deglaciation between similar to 15 and$\backslash$nsimilar to 12 ka. In the unscoured lowlands, the cosmogenic exposure$\backslash$nages have multiple modes ranging between similar to 12 and similar$\backslash$nto 50 ka, suggesting multiple periods of cold-based ice cover during$\backslash$nthe last glacial cycle. In landscapes covered by cold-based ice,$\backslash$nlarge numbers of cosmogenic exposure ages are required for elucidating$\backslash$nglacial histories.},
  doi          = {10.1139/e04-102},
  file         = {Briner2005 - Cosmogenic exposure dating in arctic glacial landscapes_ implications for the glacial history of northeastern Baffin Island, Arctic Canada.pdf:Briner2005 - Cosmogenic exposure dating in arctic glacial landscapes_ implications for the glacial history of northeastern Baffin Island, Arctic Canada.pdf:PDF;Briner2005 - Cosmogenic exposure dating in arctic glacial landscapes_ implications for the glacial history of northeastern Baffin Island, Arctic Canada.pdf:pdf2/Briner et al/Briner2005 - Cosmogenic exposure dating in arctic glacial landscapes_ implications for the glacial history of northeastern Baffin Island, Arctic Canada.pdf:PDF;Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:pdf2/Briner et al/Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:PDF;Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:Briner2005a - Cosmogenic exposure dating of late Pleistocene moraine stabilization in Alaska.pdf:PDF},
  isbn         = {0008-4077},
  journaltitle = {Canadian Journal of Earth Sciences},
}

@Article{Briner2006,
  author       = {Briner, Jason P. and Miller, Gifford H. and Davis, P. Thompson and Finkel, Robert C.},
  title        = {{Cosmogenic radionuclides from fiord landscapes support differential erosion by overriding ice sheets}},
  year         = {2006},
  volume       = {118},
  number       = {3-4},
  pages        = {406--420},
  issn         = {0016-7606},
  abstract     = {The interpretation of differentially weathered mountainous areas$\backslash$nalong the fringes of Pleistocene ice sheets is fundamental for determining$\backslash$nice-sheet behavior and thickness during the last glaciation. Two$\backslash$nexisting interpretations are either that highly weathered uplands$\backslash$nremained its nunataks while freshly eroded troughs held outlet glaciers$\backslash$nduring the last glaciation or that uplands and lowlands were equally$\backslash$ncovered by ice, but that it was differentially erosive as a function$\backslash$nof its spatially variable basal thermal regime. Cosmogenic radionuclide$\backslash$nmeasurements from 33 bedrock samples and 27 upland erratics front$\backslash$ndifferentially weathered fiord landscapes on northeastern Baffin$\backslash$nIsland shed light on Laurentide Ice Sheet (LIS) dynamics and thickness.$\backslash$nTors on weathered upland surfaces have minimum Be-10 ages between$\backslash$nca. 50 and ca. 170 ka (n = 12), whereas the majority of erratics$\backslash$nperched in the uplands range front ca. 10 to ca. 13 ka (n = 14),$\backslash$nindicating that the whole landscape was glaciated during the Last$\backslash$nGlacial Maximum (LGM). Glacially sculpted bedrock (n = 7) near sea$\backslash$nlevel reflects the age of deglaciation, and increasing amounts of$\backslash$nisotopic inheritance in high-elevation sculpted bedrock (n = 3),$\backslash$nhigher elevation intermediately weathered bedrock (n = 11), and highest$\backslash$nelevation intensely weathered bedrock reflect the weakening of erosive$\backslash$npower as the ice sheet transitioned from fiords to interfiord plateaus.$\backslash$nCold-based glaciation of uplands was contemporaneous with warm-based$\backslash$nglaciation in fiords, suggesting the presence of ice streams. Al-26$\backslash$nand Be-10 concentrations measured in 10 bedrock samples indicate$\backslash$nthat tors have experienced a complex history of alternating periods$\backslash$nof exposure, burial, and limited glacial erosion. The paired isotope$\backslash$ndata reveal a minimum duration of burial of 80-500 k.y., presumably$\backslash$nby cold-based ice, and indicate that the tors have an age of at least$\backslash$n150-580 ka. These data, together with other reconstructions of ice$\backslash$nstreams in the eastern Canadian Arctic, suggest that ice streams$\backslash$nwere a large influence on northeastern LIS dynamics throughout the$\backslash$nlate Quaternary. Thus, the northeastern LIS was sensitively tied$\backslash$nwith North Atlantic thermohaline circulation and abrupt climate change.},
  doi          = {10.1130/B25716.1},
  file         = {Briner2006 - Cosmogenic radionuclides from fiord landscapes support differential erosion by overriding ice sheets.pdf:pdf2/Briner et al/Briner2006 - Cosmogenic radionuclides from fiord landscapes support differential erosion by overriding ice sheets.pdf:PDF;Briner2006 - Cosmogenic radionuclides from fiord landscapes support differential erosion by overriding ice sheets.pdf:Briner2006 - Cosmogenic radionuclides from fiord landscapes support differential erosion by overriding ice sheets.pdf:PDF},
  isbn         = {0016-$7606\backslash$r1943-2674},
  journaltitle = {Bulletin of the Geological Society of America},
  keywords     = {Basal thermal regime,Cosmogenic radionuclide,Fjord,Glacier erosion,Laurentide ice sheet,Weathering zone},
}

@Article{Briner2008,
  author       = {Briner, Jason P. and Miller, Gifford H. and Finkel, Robert and Hess, Dale P.},
  title        = {{Glacial erosion at the fjord onset zone and implications for the organization of ice flow on Baffin Island, Arctic Canada}},
  year         = {2008},
  volume       = {97},
  number       = {1-2},
  pages        = {126--134},
  issn         = {0169-555X},
  abstract     = {Organized conduits of fast ice flow at the fringe of present and past ice sheets move the majority of ice from ice sheet interior to periphery. These relatively narrow corridors play a critical role in feedbacks between landscape evolution and ice sheet dynamics. Here we combine bedrock 10Be concentrations with distribution of lakes on Baffin Island, Arctic Canada, to constrain the pattern of glacial modification at a fjord onset zone - the region of increasing relief from the continental interior to the fjord head. High lake density and a lack of 10Be inheritance in the valley bottom indicate significant glacial scouring for some duration of the last glacial cycle. In contrast, 10Be inheritance and a lack of lakes at higher elevations indicate a lack of glacial scour and only slight glacial modification despite the entire region being covered by the Laurentide Ice Sheet during the last glaciation. This pattern of glacial erosion at the fjord onset zone is similar to what is found in the outer fjords ??? 100??km toward the last glacial maximum ice margin, and indicates that ice flow becomes organized well inland from the fjorded coast. The relief-generating process of selective linear erosion occurs at the fjord onset zone and was responsible for evolving Baffin Island fjords even in the late Quaternary. ?? 2007 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.geomorph.2007.02.039},
  file         = {Briner2008 - Glacial erosion at the fjord onset zone and implications for the organization of ice flow on Baffin Island, Arctic Canada.pdf:pdf2/Briner et al/Briner2008 - Glacial erosion at the fjord onset zone and implications for the organization of ice flow on Baffin Island, Arctic Canada.pdf:PDF;Briner2008 - Glacial erosion at the fjord onset zone and implications for the organization of ice flow on Baffin Island, Arctic Canada.pdf:Briner2008 - Glacial erosion at the fjord onset zone and implications for the organization of ice flow on Baffin Island, Arctic Canada.pdf:PDF},
  isbn         = {0169-555X},
  journaltitle = {Geomorphology},
  keywords     = {Cosmogenic radionuclides,Fjord,Glacial erosion,Ice sheet},
}

@Article{Briner2001,
  author       = {Briner, Jason P and Swanson, Terry W and Caffee, Marc},
  title        = {{Late Pleistocene Cosmogenic 36{C}l Glacial Chronology of the Southwestern Ahklun Mountains, Alaska}},
  year         = {2001},
  volume       = {56},
  number       = {2},
  pages        = {148--154},
  issn         = {0033-5894},
  abstract     = {Thirty-two cosmogenic 36Cl surface exposure ages constrain the timing of two late Pleistocene glacial advances in the western Ahklun Mountains, southwestern Alaska. Boulders were sampled from one early Wisconsin (sensu lato) and six late Wisconsin moraines deposited by ice-cap outlet glaciers and local alpine glaciers. Four moraine boulders deposited during an extensive early Wisconsin ice-cap outlet glacier advance have a mean surface exposure age of 60,300+-3200 yr. A moraine deposited by an ice-cap outlet glacier during the restricted late Wisconsin advance has a mean surface exposure age of 19,600+-1400 yr. Five moraines deposited by late Wisconsin alpine glaciers have mean ages that range between 30,000 and 17,000 yr. The 36Cl ages are consistent with limiting 14C and thermoluminescence ages from related deposits and indicate that Ahklun Mountains glaciers reached their most extensive position of the last glaciation early during the late Pleistocene, in contrast to the deep-sea isotopic record of global ice volume. Copyright 2001 University of Washington.},
  doi          = {DOI: 10.1006/qres.2001.2255},
  file         = {Briner2001 - Late Pleistocene Cosmogenic 36Cl Glacial Chronology of the Southwestern Ahklun Mountains, Alaska.pdf:Briner2001 - Late Pleistocene Cosmogenic 36Cl Glacial Chronology of the Southwestern Ahklun Mountains, Alaska.pdf:PDF;Briner2001 - Late Pleistocene Cosmogenic 36Cl Glacial Chronology of the Southwestern Ahklun Mountains, Alaska.pdf:pdf2/Briner, Swanson, Caffee/Briner2001 - Late Pleistocene Cosmogenic 36Cl Glacial Chronology of the Southwestern Ahklun Mountains, Alaska.pdf:PDF},
  isbn         = {0033-5894},
  journaltitle = {Quaternary Res.},
  keywords     = {36 cl,alaska,cosmogenic,glacial chronology},
  pmid         = {2001075291},
}

