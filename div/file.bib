@Article{Ammon1991,
  author       = {Ammon, CJ},
  title        = {{The isolation of receiver effects from teleseismic P waveforms}},
  year         = {1991},
  volume       = {81},
  number       = {6},
  pages        = {2504--2510},
  issn         = {0037-1106},
  abstract     = {Time-domain receiver function modeling (Burdick and Langston, 1977; Langston, 1977, 1979, 1981) was developed to model the shear-wave converted phases and multiples recorded on the horizontal components of P waveforms. Early researchers utilized long-period WWSSN data to construct simple plane- layered models or models containing a few moderately dipping planar inter- faces. Extension to higher frequencies introduced the need for more detailed (thinner layers) models and a linearized time-domain inversion technique (Owens et al., 1984). (For a general review of the receiver function modeling technique, see Langston, 1979; Owens et al., 1984; and Ammon et al., 1990.) In this note, I concentrate on recent developments related to the procedure used to isolate the near-receiver propagation effects from the far-field dislocation time function, near-source structure, lower mantle propagation effects. I begin with a review of Langston's (1979) deconvolution procedure and clear up a recent misinterpretation of the zero-lag arrival on the receiver function as simply a measure of the correlation between the vertical and radial components of motion. This arrival has a physical significance associated with the wave propagation through the simple one-dimensional models assumed in most re- ceiver function studies. I also describe a simple addition to Langston's (1979) equalization technique, which enables an estimate of the absolute amplitude of the receiver function. This absolute amplitude provides an additional constraint on the near-surface velocity structure. Additionally, working with true ampli- tudes illustrates the decrease in the amplitude of the converted phases with increasing source-receiver distance. The true amplitudes are important when estimating the influence of scattering on the receiver function estimates. RECEIVER},
  doi          = {10.1029/2005JB004161},
  isbn         = {0037-1106},
  journaltitle = {Bulletin of the Seismological Society of America},
}

@Article{Ammon1990,
  author       = {Ammon, Charles J and Randall, George E},
  title        = {{On the Nonuniqueness of Receiver Function Inversions}},
  year         = {1990},
  volume       = {95},
  number       = {B10},
  pages        = {15,303--15,3318},
  issn         = {0148-0227},
  abstract     = {To stiady the resolving power of teleseismic P waveforms for receiver structure, we model synthetic waveforms using a time domain waveform inversion scheme beginning with a range of initial models to estimate the range of acceptable velocity structures. To speed up the waveform inversions, we implement Randali's (1989) efficient algorithms for calculating differential seismograms and include a smoothness constraint on all the resulting velocity models utilizing the "jumping" inversion technique of Shaw and Orcutt (1985). We present the results of more than 235 waveform inversions for one-dimensional velocity structures that indicate that the primary sensitivity of a receiver function is to high wavenumber velocity changes, and a depth-velocity product, not simply velocity. The range of slownesses in a typical receiver function study does not appear to be broad enough to remove the depth-velocity ambiguity; the inclusion of a priori information is necessary. We also present inversion results for station RSCP, located in the Cumberland Plateau, Tennessee. Our results are similar to those from a previous study by Owens et al. (1984) and demonstrate the uncertainties in the resulting velocity estimate more clearly. INTRODUCTION},
  doi          = {10.1029/JB095iB10p15303},
  file         = {Ammon1990 - On the Nonuniqueness of Receiver Function Inversions.pdf:Ammon1990 - On the Nonuniqueness of Receiver Function Inversions.pdf:PDF;Ammon1990 - On the Nonuniqueness of Receiver Function Inversions.pdf:pdf2/Ammon, Randall/Ammon1990 - On the Nonuniqueness of Receiver Function Inversions.pdf:PDF},
  isbn         = {0148-0227},
  journaltitle = {Journal of Geophysical Research},
  keywords     = {http://dx.doi.org/10.1029/JB095iB10p15303, doi:10.},
}

