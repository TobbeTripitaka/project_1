@Article{USGS2009,
  author       = {USGS and NOAA and NSF},
  title        = {{Past Climate Variability and Change in the Arctic and at High Latitudes}},
  year         = {2009},
  volume       = {Synthesis},
  number       = {January},
  pages        = {462},
  abstract     = {Introduction Paleoclimate records play a key role in our understanding of Earths past and present climate system and in our confidence in predicting future climate changes. Paleoclimate data help to elucidate past and present active mechanisms of climate change by placing the short instrumental record into a longer term context and by permitting models to be tested beyond the limited time that instrumental measurements have been available. Recent observations in the Arctic have identified large ongoing changes and important climate feedback mechanisms that multiply the effects of global-scale climate changes. Ice is especially important in these Arctic amplification processes, which also involve the ocean, the atmosphere, and the land surface (vegetation, soils, and water). As discussed in this report, paleoclimate data show that land and sea ice have grown with cooling temperatures and have shrunk with warming ones, amplifying temperature changes while causing and responding to ecosystem shifts and sea-level changes.},
  file         = {USGS2009 - Past Climate Variability and Change in the Arctic and at High Latitudes.pdf:pdf2/USGS, NOAA, NSF/USGS2009 - Past Climate Variability and Change in the Arctic and at High Latitudes.pdf:PDF;USGS2009 - Past Climate Variability and Change in the Arctic and at High Latitudes.pdf:USGS2009 - Past Climate Variability and Change in the Arctic and at High Latitudes.pdf:PDF},
  journaltitle = {U.S. Climate Change Science Program},
}

