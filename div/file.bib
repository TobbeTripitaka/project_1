@Article{Duque2011,
  author       = {Duque, Carlos Augusto and Silveira, Paulo M. and Ribeiro, Paulo F.},
  title        = {{Visualizing time-varying harmonics using filter banks}},
  year         = {2011},
  volume       = {81},
  number       = {4},
  pages        = {974--983},
  issn         = {0378-7796},
  abstract     = {Although it is well known that Fourier analysis is in reality only accurately applicable to steady state waveforms, it is a widely used tool to study and monitor time-varying signals, such as are commonplace in electrical power systems. The disadvantages of Fourier analysis, such as frequency spillover or problems due to sampling (data window) truncation can often be minimized by various windowing techniques, but they nevertheless exist. This paper demonstrates that it is possible to track and visualize amplitude and time-varying power systems harmonics, without frequency spillover caused by time-frequency techniques. This new tool allows for a clear visualization of time-varying harmonics, which can lead to better ways to track harmonic distortion and understand time-dependent power quality parameters. It also has the potential to assist with control, protection and power quality applications. ?? 2010 Elsevier B.V.},
  doi          = {10.1016/j.epsr.2010.11.030},
  file         = {Duque2011 - Visualizing time-varying harmonics using filter banks.pdf:pdf2/Duque, Silveira, Ribeiro/Duque2011 - Visualizing time-varying harmonics using filter banks.pdf:PDF;Duque2011 - Visualizing time-varying harmonics using filter banks.pdf:Duque2011 - Visualizing time-varying harmonics using filter banks.pdf:PDF},
  journaltitle = {Electric Power Systems Research},
  keywords     = {Filter banks,Harmonic decomposition,Harmonic distortion,Multi-resolution analysis,Time-varying power harmonic distortion,Wavelet transform},
  publisher    = {Elsevier B.V.},
}

