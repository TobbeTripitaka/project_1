@Article{Hirth2000,
  author       = {Hirth, Greg and Evans, Rob L. and Chave, Alan D.},
  title        = {{Comparison of continental and oceanic mantle electrical conductivity: Is the Archean lithosphere dry?}},
  year         = {2000},
  volume       = {1},
  number       = {12},
  issn         = {1525-2027},
  abstract     = {Electrical conductivity profiles derived from magnetotelluric and geomagnetic sounding methods provide a means of constraining upper mantle properties that is complementary to seismic studies. We analyze conductivity profiles for an Archean craton and an oceanic setting, in conjunction with independent constraints on mantle geotherms, to estimate the water content of the upper mantle in these very different geologic environments. Results from this analysis indicate that the Archean lithosphere contains less water than the oceanic mantle in the depth range between $\sim$150 and $\sim$250 km. Below $\sim$250 km these oceanic and continental environments show similar conductivities, suggesting that the water content of the mantle does not vary significantly between $\sim$250 km and the 410-km discontinuity. These observations indicate that the Archean lithosphere may be stabilized against convective instabilities partly because it has a high viscosity associated with a dry composition.},
  doi          = {10.1029/2000GC000048},
  file         = {Hirth2000 - Comparison of continental and oceanic mantle electrical conductivity_ Is the Archean lithosphere dry_.pdf:Hirth2000 - Comparison of continental and oceanic mantle electrical conductivity_ Is the Archean lithosphere dry_.pdf:PDF;Hirth2000 - Comparison of continental and oceanic mantle electrical conductivity_ Is the Archean lithosphere dry_.pdf:pdf2/Hirth, Evans, Chave/Hirth2000 - Comparison of continental and oceanic mantle electrical conductivity_ Is the Archean lithosphere dry_.pdf:PDF},
  isbn         = {1525-2027},
  journaltitle = {Geochemistry, Geophysics, Geosystems},
  keywords     = {Electrical conductivity,continental lithosphere,craton,mantle,water},
}

@Article{Hjuler2009,
  author       = {Hjuler, M. L. and Fabricius, I. L.},
  title        = {{Engineering properties of chalk related to diagenetic variations of Upper Cretaceous onshore and offshore chalk in the North Sea area}},
  year         = {2009},
  volume       = {68},
  number       = {3-4},
  pages        = {151--170},
  issn         = {0920-4105},
  abstract     = {Predicting properties of reservoir chalk involve testing and modelling of easily obtainable outcrop chalk. However, variations in onshore and offshore chalk properties will expectedly occur due to different diagenetic history. Understanding diagenetic mechanisms and their consequences are thus a key issue when reservoir chalk characterization is based on outcrop chalk. In chalk of the Valhall field high pore pressure and hydrocarbon presence have counteracted recrystallization and cementation and preserved high porosity despite a burial depth of c. 3 km. As a consequence inter-particle connections are poorly developed and Valhall chalk is expectedly geomechanically weak. Similar high porosity and weak consolidation were observed in Danish outcrop chalk which expectedly will match the Valhall samples geomechanically. In chalk of the Dan, South Arne and Ekofisk fields recrystallization and cementation features are significantly better developed compared to the Valhall field. Calcite redistribution has strengthened particle contacts and reduced porosity, assumingly providing a geomechanically stronger chalk. Similar low porosity and well-developed recrystallization and cementation features are found in English chalk which from a geomechanical point of view may share characteristics with chalk of the Dan, South Arne and Ekofisk fields. Matrix permeability is related not only to porosity but also to specific surface area of chalk which is controlled partly by calcite recrystallization partly by type and amount of non-carbonate minerals. In onshore chalk significant mineralogical differences are reflected in varying specific surface area. In contrast, specific surface area of Upper Cretaceous reservoir chalk show little variation indicating that care must be taken when choosing an outcrop chalk as substitute for reservoir chalk during flooding experiments. ?? 2009 Elsevier B.V. All rights reserved.},
  doi          = {10.1016/j.petrol.2009.06.005},
  file         = {Hjuler2009 - Engineering properties of chalk related to diagenetic variations of Upper Cretaceous onshore and offshore chalk in the North Sea area.pdf:Hjuler2009 - Engineering properties of chalk related to diagenetic variations of Upper Cretaceous onshore and offshore chalk in the North Sea area.pdf:PDF;Hjuler2009 - Engineering properties of chalk related to diagenetic variations of Upper Cretaceous onshore and offshore chalk in the North Sea area.pdf:pdf2/Hjuler, Fabricius/Hjuler2009 - Engineering properties of chalk related to diagenetic variations of Upper Cretaceous onshore and offshore chalk in the North Sea area.pdf:PDF},
  isbn         = {0920-4105},
  journaltitle = {Journal of Petroleum Science and Engineering},
  keywords     = {North Sea,cohesion,flooding properties,geomechanical properties,outcrop chalk,reservoir chalk},
  publisher    = {Elsevier B.V.},
}

@Article{Hoffman1991,
  author       = {Hoffman, P.F.},
  title        = {{Did the breakout of Laurentia turn Gondwanaland inside-out?}},
  year         = {1991},
  volume       = {252},
  number       = {5011},
  pages        = {1409--1412},
  issn         = {0036-8075},
  abstract     = {Comparative geology suggests that the continents adjacent to northern, western, southern, and eastern Laurentia in the Late Proterozoic were Siberia, Australia-Antarctica, southern Africa, and Amazonia-Baltica, respectively. Late Proterozoic fragmentation of the supercontinent centered on Laurentia would then have been followed by rapid fan-like collapse of the (present) southern continents and eventual consolidation of East and West Gondwanaland. In this scenario, a pole of rotation near the Weddell Sea would explain the observed dominance of wrench tectonics in (present) east-west trending Pan-African mobile belts and subduction-accretion tectonics in north-south trending belts. In the process of fragmentation, rifts originating in the interior of the Late Proterozoic supercontinent became the external margins of Paleozoic Gondwanaland; exterior margins of the Late Proterozoic supercontinent became landlocked within the interior of Gondwanaland.},
  doi          = {10.1126/science.252.5011.1409},
  file         = {Hoffman1991 - Did the breakout of Laurentia turn Gondwanaland inside-out_.pdf:Hoffman1991 - Did the breakout of Laurentia turn Gondwanaland inside-out_.pdf:PDF;Hoffman1991 - Did the breakout of Laurentia turn Gondwanaland inside-out_.pdf:pdf2/Hoffman/Hoffman1991 - Did the breakout of Laurentia turn Gondwanaland inside-out_.pdf:PDF},
  isbn         = {0036-8075},
  journaltitle = {Science},
  pmid         = {17772912},
}

@Article{Hoefle2000,
  author       = {H{\"{o}}fle, Claudia and Edwards, Mary E. and Hopkins, David M. and Mann, Daniel H. and Ping, Chien-Lu},
  title        = {{The Full-Glacial Environment of the Northern Seward Peninsula, Alaska, Reconstructed from the 21,500-Year-Old Kitluk Paleosol}},
  year         = {2000},
  volume       = {53},
  number       = {2},
  pages        = {143--153},
  issn         = {0033-5894},
  abstract     = {Paleoenvironmental conditions are reconstructed from soils bur- ied under volcanic ash ca. 21,500 years ago on the Seward Penin- sula. Soil development was minimal, reflecting the continuous regional deposition of loess, which originated from river flood- plains and the exposed Chukchi shelf. Cryoturbated soil horizons, ice wedges, and ice-lens formation indicate a permafrost environ- ment and mean annual temperatures below 6{$^{\circ}$} to 8{$^{\circ}$}C. Shallow active layers (average 45 cm), minimal evidence for chemical leaching of soils, and the presence of earthen hummocks indicate a cold and seasonally dry climate. Neither steppe nor polar desert soils are appropriate analogues for these zonal soils of loess- covered central Beringia. No exact analogues are known; however, soils underlying dry tundra near the arctic coast of northern Yakutia, Russia, and under moist, nonacidic tundra of the Alas- kan North Slope have properties in common with the buried soils.},
  doi          = {10.1006/qres.1999.2097},
  journaltitle = {Quaternary Research},
  keywords     = {bering land bridge,beringia,last glacial,paleoenvironmental reconstruction,paleosols,permafrost soils},
}

