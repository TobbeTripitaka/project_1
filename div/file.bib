@Article{Abe-Ouchi2007,
  author       = {Abe-Ouchi, a. and Segawa, T. and Saito, F.},
  title        = {{Climatic conditions for modelling the Northern Hemisphere ice sheets throughout the ice age cycle}},
  journal      = {Climate of the Past Discussions},
  year         = {2007},
  volume       = {3},
  number       = {1},
  pages        = {301--336},
  month        = feb,
  issn         = {1814-9332},
  abstract     = {The ice sheet-climate interaction as well as the climatic response to orbital parameters and atmospheric CO2 concentration are examined in order to drive an ice sheet model throughout an ice age cycle. Feedback processes between ice sheet and atmosphere are analyzed by numerical experiments using a high resolution General Circulation Model (GCM) under different conditions at the Last Glacial Maximum. Among the proposed processes, the ice albedo feedback, the elevation-mass balance feedback and the desertification effect over the ice sheet were found to be the dominant processes for the ice-sheet mass balance. For the elevation-mass balance feedback, the temperature lapse rate over the ice sheet is proposed to be weaker than assumed in previous studies. Within the plausible range of parameters related to these processes, the ice sheet response to the orbital parameters and atmospheric CO2 concentration for the last glacial/interglacial cycle was simulated in terms of both ice volume and geographical distribution, using a threedimensional ice-sheet model. Careful treatment of climate ice sheet feedback is essential for a reliable simulation of the ice sheet changes during ice age cycles.},
  doi          = {10.5194/cpd-3-301-2007},
  file         = {Abe-Ouchi2007 - Climatic conditions for modelling the Northern Hemisphere ice sheets throughout the ice age cycle.pdf:Abe-Ouchi2007 - Climatic conditions for modelling the Northern Hemisphere ice sheets throughout the ice age cycle.pdf:PDF},
  isbn         = {1814-9324},
  journaltitle = {Climate of the Past Discussions},
  publisher    = {Copernicus {GmbH}},
}

