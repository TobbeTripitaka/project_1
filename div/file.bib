  year         = {1994},
  volume       = {6},
  pages        = {85--92},
  file         = {Hole1994a - Tectonic controls on the geochemical composition of Cenozoic, mafic alkaline volcanic rocks from West Antarctica.pdf:Hole1994a - Tectonic controls on the geochemical composition of Cenozoic, mafic alkaline volcanic rocks from West Antarctica.pdf:PDF;Hole1994a - Tectonic controls on the geochemical composition of Cenozoic, mafic alkaline volcanic rocks from West Antarctica.pdf:pdf2/Hole, Lemasurier/Hole1994a - Tectonic controls on the geochemical composition of Cenozoic, mafic alkaline volcanic rocks from West Antarctica.pdf:PDF;Hole1994 - Tectonic setting and geochemistry of Miocene alkalic basalts from the Jones Mountains, West Antarctica.pdf:pdf2/Hole, Storey, LeMasurier/Hole1994 - Tectonic setting and geochemistry of Miocene alkalic basalts from the Jones Mountains, West Antarctica.pdf:PDF;Hole1994 - Tectonic setting and geochemistry of Miocene alkalic basalts from the Jones Mountains, West Antarctica.pdf:Hole1994 - Tectonic setting and geochemistry of Miocene alkalic basalts from the Jones Mountains, West Antarctica.pdf:PDF},
