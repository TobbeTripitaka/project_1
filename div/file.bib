@Article{Nishi2014,
  author       = {Nishi, Masayuki},
  title        = {{Deep water cycle: Mantle hydration}},
  year         = {2014},
  volume       = {8},
  number       = {1},
  pages        = {9--10},
  issn         = {1752-0894},
  abstract     = {The fate of water that enters the mantle within subducting slabs is unclear. Laboratory experiments indicate that subducted crust can transport large amounts of water into the deep Earth, and the lower mantle may become more hydrated over time.},
  doi          = {10.1038/ngeo2326},
  file         = {Nishi2014 - Deep water cycle_ Mantle hydration.pdf:pdf2/Nishi/Nishi2014 - Deep water cycle_ Mantle hydration.pdf:PDF;Nishi2014a - Stability of hydrous silicate at high pressures and water transport to the deep lower mantle.pdf:Nishi2014a - Stability of hydrous silicate at high pressures and water transport to the deep lower mantle.pdf:PDF;Nishi2014 - Deep water cycle_ Mantle hydration.pdf:Nishi2014 - Deep water cycle_ Mantle hydration.pdf:PDF;Nishi2014a - Stability of hydrous silicate at high pressures and water transport to the deep lower mantle.pdf:pdf2/Nishi et al/Nishi2014a - Stability of hydrous silicate at high pressures and water transport to the deep lower mantle.pdf:PDF},
  isbn         = {1752-$0894\backslash$r1752-0908},
