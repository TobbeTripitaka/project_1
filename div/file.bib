@Article{Lenton2008,
  author       = {Lenton, Timothy M and Held, Hermann and Kriegler, Elmar and Hall, Jim W and Lucht, Wolfgang and Rahmstorf, Stefan and Schellnhuber, Hans Joachim},
  title        = {{Tipping elements in the Earth's climate system}},
  year         = {2008},
  volume       = {105},
  number       = {6},
  pages        = {1786--1793},
  issn         = {1091-6490},
  abstract     = {The term {\textquoteleft}{\textquoteleft}tipping point'' commonly refers to a critical threshold at which a tiny perturbation can qualitatively alter the state or development of a system. Here we introduce the term {\textquoteleft}{\textquoteleft}tipping element'' to describe large-scale components of the Earth system that may pass a tipping point. We critically evaluate potential policy-relevant tipping elements in the climate system under anthropogenic forcing, drawing on the pertinent literature and a recent international workshop to compile a short list, and we assess where their tipping points lie. An expert elicitation is used to help rank their sensitivity to global warming and the uncertainty about the underlying physical mechanisms. Then we explain how, in principle, early warning systems could be established to detect the proximity of some tipping points.},
  doi          = {10.1073/pnas.0705414105},
  file         = {Lenton2008 - Tipping elements in the Earth's climate system.pdf:Lenton2008 - Tipping elements in the Earth's climate system.pdf:PDF;Lenton2008 - Tipping elements in the Earth's climate system.pdf:pdf2/Lenton et al/Lenton2008 - Tipping elements in the Earth's climate system.pdf:PDF},
  isbn         = {1091-6490 (Electronic) 0027-8424 (Linking)},
  journaltitle = {Proceedings of the National Academy of Sciences},
  pmid         = {18258748},
}

@Article{Lerche1997,
  author       = {Lerche, I},
  title        = {{Erosion and uplift uncertainties in the Barents Sea, Norway}},
  year         = {1997},
  volume       = {29},
  number       = {4},
  pages        = {469--501},
  issn         = {0882-8121},
  abstract     = {Abstract  Three different types of methods are used to assess the ability to determine erosion amounts and to provide estimates of uncertainty. In the situation of dynamical indicator methods, such as seismic velocity, sonic logs, density logs, or drilling exponent methods, intrinsic assumptions and parameter values used provide only a broad statement on the resolution of uplift/erosion events. None of the methods is more accurate, at best, to better than $\pm$ 1 km and likely much worse. For geological model procedures, exemplified by considerations of Airy isostasy and by bed upturning near a salt dome in the Nordkapp Basin of the Barents Sea, the uncertainties are again of the order of $\pm$ 500--1000 m. With thermal indicator procedures, the bulk of the constraint information from available data is needed to determine paleoheat flux with little left over to constrain the erosion, implying a minimum uncertainty of $\pm$ 500 m on erosion determinations. No method seems capable of resolving erosional events to better than a minimum uncertainty of $\pm$ 500 m, and likely no better than $\pm$ 1 km.},
  doi          = {10.1007/BF02775084},
  file         = {Lerche1997 - Erosion and uplift uncertainties in the Barents Sea, Norway.pdf:Lerche1997 - Erosion and uplift uncertainties in the Barents Sea, Norway.pdf:PDF;Lerche1997 - Erosion and uplift uncertainties in the Barents Sea, Norway.pdf:pdf2/Lerche/Lerche1997 - Erosion and uplift uncertainties in the Barents Sea, Norway.pdf:PDF},
  journaltitle = {Mathematical Geology},
  keywords     = {barents sea,erosion,uncertainty},
  pmid         = {25226},
}

