  author       = {Dowdeswell, J. A. and Ottesen, D. and Evans, J. and Cofaigh, C. {\'{O}} and Anderson, J. B.},
  title        = {{Submarine glacial landforms and rates of ice-stream collapse}},
  year         = {2008},
  volume       = {36},
