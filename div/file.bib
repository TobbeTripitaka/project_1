@Article{Thybo1997,
  author       = {Thybo, H.},
  title        = {{The Seismic 8 Discontinuity and Partial Melting in Continental Mantle}},
  year         = {1997},
  volume       = {275},
  number       = {5306},
  pages        = {1626--1629},
  issn         = {0036-8075},
  abstract     = {Strong, scattered reflections beyond 8 degrees (8degrees) offset are characteristic features of all high-resolution seismic sections from the continents. The reflections identify a low-velocity zone below approximately 100 kilometers depth beneath generally stratified mantle. This zone may be caused by partial melting, globally initiated at equal depth in the continental mantle. Solid state is again attained at the Lehmann discontinuity in cold, stable areas, whereas the zone extends to near the 400-kilometer discontinuity in hot, tectonically active areas. Thus, the depth to the Lehmann discontinuity may be an indicator of the thermal state of the continental mantle.},
  doi          = {10.1126/science.275.5306.1626},
  file         = {Thybo1997 - The Seismic 8 Discontinuity and Partial Melting in Continental Mantle.pdf:pdf2/Thybo/Thybo1997 - The Seismic 8 Discontinuity and Partial Melting in Continental Mantle.pdf:PDF;Thybo1997 - The Seismic 8 Discontinuity and Partial Melting in Continental Mantle.pdf:Thybo1997 - The Seismic 8 Discontinuity and Partial Melting in Continental Mantle.pdf:PDF},
  isbn         = {0036-8075},
  journaltitle = {Science},
  pmid         = {9054353},
}

@Article{Tikku2000,
  author       = {Tikku, Anahita A. and Cande, Steven C.},
  title        = {{On the fit of Broken Ridge and Kerguelen Plateau}},
  year         = {2000},
  volume       = {180},
  number       = {1-2},
  pages        = {117--132},
  issn         = {0012-821X},
  abstract     = {We present revised finite rotation parameters for Australia and East Antarctica in the Early Tertiary incorporating new magnetic data off of Wilkes Land, Antarctica. Reconstructions based on these and recently published rotation parameters document a relatively stationary position of Broken Ridge abutted against Central Kerguelen Plateau between chron 34 (83 Ma) and chron 20 (43 Ma), prior to the commencement of seafloor spreading between the two Late Cretaceous features. These reconstructions are significant as they are the first which are consistent with geological and geophysical constraints in not producing an overlap of Broken Ridge and Kerguelen Plateau between chron 34 and chron 24 (53 Ma) and not requiring significant extension or strike-slip motion within Kerguelen Plateau. We also find that the finite rotations for chrons 20 to 34 outline the area of rough topography inferred from the satellite-derived free-air gravity for the Diamantina Zone and Labuan Basin. A consequence of these rotations is that there is considerable overlap of continental crust in the region of Tasmania, the South Tasman Rise and East Antarctica in the Late Cretaceous. Solutions to this problem may imply either post-Santonian ($\leq$79 Ma) extension or transform motion in the Bass Strait, or late Cretaceous or Cenozoic extension in Wilkes Land. The reconstructions also agree with prior estimates of roughly 150 km of motion between East and West Antartica since chron 20. (C) 2000 Elsevier Science B.V. All rights reserved.},
  doi          = {10.1016/S0012-821X(00)00157-6},
  file         = {Tikku2000 - On the fit of Broken Ridge and Kerguelen Plateau.pdf:pdf2/Tikku, Cande/Tikku2000 - On the fit of Broken Ridge and Kerguelen Plateau.pdf:PDF;Tikku2000 - On the fit of Broken Ridge and Kerguelen Plateau.pdf:Tikku2000 - On the fit of Broken Ridge and Kerguelen Plateau.pdf:PDF},
  journaltitle = {Earth and Planetary Science Letters},
  keywords     = {Antarctica,Australia,Broken Ridge,Cretaceous,Indian Ocean,Kerguelen Plateau,Magnetic anomalies,Sea-floor spreading,Tertiary},
}

@Article{Tikku1999,
  author       = {Tikku, Anahita a. and Cande, Steven C.},
  title        = {{The oldest magnetic anomalies in the Australian-Antarctic Basin: Are they isochrons?}},
  year         = {1999},
  volume       = {104},
  number       = {B1},
  pages        = {661},
  issn         = {0148-0227},
  abstract     = {We present a revised tectonic interpretation of Australia and Antarctica incorporating new magnetic data off of Wilkes Land, Antarctica, for the earliest period of seafloor spreading on the Southeast Indian Ridge, from the Late Cretaceous to Early Tertiary. Reconstructions based on our revised anomaly identifications are characterized by a surprisingly large amount of continental overlap, involving the South Tasman Rise, Tasmania, and the Victoria Land region of Antarctica. The overlap is due to continental extension and/or deformation of oceanic crust in the Australian-Antarctic Basin. The second hypothesis would imply that the magnetic anomalies older than chron 31 in the Australian-Antarctic Basin are not isochrons. We also fmd evidence for a previously unrecognized period of extremely slow spreading on the Southeast Indian Ridge from chron 31 (68.7 Ma) to chron 24 (53.3 Ma) in the Australian- Antarctic Basin. In the western end of the basin we have tentatively identified a hiatus in the generation of crustal accretion associated with a minimum sustainable threshold half spreading rate of 1.5 mm/yr. In addition, we recognize a new fracture zone on the Antarctic plate, the Vincennes fracture zone, as the conjugate to the Perth fracture zone on the Australian plate, in the western Australian-Antarctic Basin. These fracture zones record the initial NW-SE opening direction between Australia and Antarctica.},
  doi          = {10.1029/1998JB900034},
  file         = {Tikku1999 - The oldest magnetic anomalies in the Australian-Antarctic Basin_ Are they isochrons_.pdf:Tikku1999 - The oldest magnetic anomalies in the Australian-Antarctic Basin_ Are they isochrons_.pdf:PDF;Tikku1999 - The oldest magnetic anomalies in the Australian-Antarctic Basin_ Are they isochrons_.pdf:pdf2/Tikku, Cande/Tikku1999 - The oldest magnetic anomalies in the Australian-Antarctic Basin_ Are they isochrons_.pdf:PDF},
  isbn         = {2169-9313},
  journaltitle = {Journal of Geophysical Research},
  keywords     = {http://dx.doi.org/10.1029/1998JB900034, doi:10.102},
}

@Article{Toggweiler2001,
  author       = {Toggweiler, Jr and Key, Rm},
  title        = {{Ocean circulation: Thermohaline circulation}},
  year         = {2001},
  volume       = {4},
  number       = {December 2007},
  pages        = {1549--1555},
  issn         = {1476-4687},
  abstract     = {The circulation of the ocean is usually divided into two parts, a wind-driven circulation that dominates in the upper few hundred meters, and a density-driven circulation that dominates below. The latter is called the {\textquoteleft}thermohaline' circulation because of the role of heating, cool- ing, freshening, and salinification in producing regional density differences within the ocean. The thermohaline circulation, for the most part, is an {\textquoteleft}overturning' circulation in which warm water flows poleward near the surface and is subsequently converted into cold water that sinks and flows equatorward in the in- terior. Radiocarbon measurements show that the thermohaline circulation turns over all the deep water in the ocean every 600 years or so},
  doi          = {10.1002/joc},
  file         = {Toggweiler2001 - Ocean circulation_ Thermohaline circulation.pdf:pdf2/Toggweiler, Key/Toggweiler2001 - Ocean circulation_ Thermohaline circulation.pdf:PDF;Toggweiler2001 - Ocean circulation_ Thermohaline circulation.pdf:Toggweiler2001 - Ocean circulation_ Thermohaline circulation.pdf:PDF},
  isbn         = {0262194929},
  journaltitle = {Encyclopedia of Atmospheric Sciences},
  keywords     = {Antarctic Circumpolar,Current,Ekman layer,Gulfstream,cline,conveyor,deep-water,formation,heat transport,hydrological cycle,pcyno-,salinity,thermocline,upwelling},
  pmid         = {23739423},
}

