@Article{Williams2011,
  author        = {Williams, Simon E. and Whittaker, Joanne M. and M{\"{u}}ller, R. Dietmar},
  title         = {{Full-fit, palinspastic reconstruction of the conjugate Australian-Antarctic margins}},
  year          = {2011},
  volume        = {30},
  number        = {6},
  pages         = {1--21},
  issn          = {0278-7407},
  __markedentry = {[tobiasstal:1]},
  abstract      = {Despite decades of study the prerift configuration and early rifting$\backslash$nhistory between Australia and Antarctica is not well established.$\backslash$nThe plate boundary system during the Cretaceous includes the evolving$\backslash$nKerguelen--Broken Ridge Large Igneous Province in the west as well$\backslash$nas the conjugate passive and transform margin segments of the Australian$\backslash$nand Antarctic continents. Previous rigid plate reconstruction models$\backslash$nhave highlighted the difficulty in satisfying all the available observations$\backslash$nwithin a single coherent reconstruction history. We investigate a$\backslash$nrange of scenarios for the early rifting history of these plates$\backslash$nby developing a deforming plate model for this conjugate margin pair.$\backslash$nPotential field data are used to define the boundaries of stretched$\backslash$ncontinental crust on a regional scale. Integrating crustal thickness$\backslash$nalong tectonic flow lines provides an estimate of the prerift location$\backslash$nof the continental plate boundary. We then use the prerift plate$\backslash$nboundary positions, along with additional constraints from geological$\backslash$nstructures and large igneous provinces within the same Australian$\backslash$nand Antarctic plate system, to compute {\textquotedblleft}full-fit{\textquotedblright} poles of rotation$\backslash$nfor Australia relative to Antarctica. Our preferred model implies$\backslash$nthat the Leeuwin and Vincennes Fracture Zones are conjugate features$\backslash$nwithin Gondwana, but that the direction of initial opening between$\backslash$nAustralia and Antarctica does not follow the orientation of these$\backslash$nfeatures; rather, the geometry of these features is likely related$\backslash$nto the earlier rifting of India away from Australia-Antarctica. Previous$\backslash$nfull-fit reconstructions, based on qualitative estimates of continental$\backslash$nmargin overlaps, generally yield a tighter fit than our preferred$\backslash$nreconstruction based on palinspastic margin restoration.},
  doi           = {10.1029/2011TC002912},
  file          = {Williams2011 - Full-fit, palinspastic reconstruction of the conjugate Australian-Antarctic margins.pdf:Williams2011 - Full-fit, palinspastic reconstruction of the conjugate Australian-Antarctic margins.pdf:PDF;Williams2011 - Full-fit, palinspastic reconstruction of the conjugate Australian-Antarctic margins.pdf:pdf2/Williams, Whittaker, Mller/Williams2011 - Full-fit, palinspastic reconstruction of the conjugate Australian-Antarctic margins.pdf:PDF},
  isbn          = {0278-7407},
  journaltitle  = {Tectonics},
}

@Article{Wilson2006,
  author       = {Wilson, Greg},
  title        = {{Software carpentry: Getting scientists to write better code by making them more productive}},
  year         = {2006},
  volume       = {8},
  number       = {6},
  pages        = {66--69},
  issn         = {1521-9615},
  abstract     = {For the past years, my colleagues and I have developed a one-semester course that teaches scientists and engineers the "common core" of modern software development. Our experience shows that an investment of 150 hours-25 of lectures and the rest of practical work-can improve productivity by roughly 20 percent. That's one day a week, one less semester in a master's degree, or one less year for a typical PhD. The course is called software carpentry, rather than software engineering, to emphasize the fact that it focuses on small-scale and immediately practical issues. All of the material is freely available under an open-source license at www.swc.scipy.org and can be used both for self-study and in the classroom. This article describes what the course contains, and why},
  doi          = {10.1109/MCSE.2006.122},
  file         = {Wilson2006 - Software carpentry_ Getting scientists to write better code by making them more productive.pdf:pdf2/Wilson/Wilson2006 - Software carpentry_ Getting scientists to write better code by making them more productive.pdf:PDF;Wilson2006 - Software carpentry_ Getting scientists to write better code by making them more productive.pdf:Wilson2006 - Software carpentry_ Getting scientists to write better code by making them more productive.pdf:PDF},
  isbn         = {15219615},
  journaltitle = {Computing in Science and Engineering},
}

